<html>
<head>
<title>LCSPortlet</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">LCSPortlet</td></tr>
</thead>
<tbody>
<tr>
	<td>INFO_MESSAGE</td>
	<td>//div[contains(@class,'lcs-alert')]</td>
	<td></td>
</tr>
<tr>
	<td>CONNECTION_STATUS</td>
	<td>//div[contains(@class,'lcs-connection-status')]/span[@class='lcs-connection-label']</td>
	<td></td>
</tr>
<tr>
	<td>PROJECT_HOME_LINK</td>
	<td>//div[@class='lcs-portal-info']//dl[contains(.,'Project Home')]//a</td>
	<td></td>
</tr>
<tr>
	<td>ENVIRONMENT_LINK</td>
	<td>//div[@class='lcs-portal-info']//dl[contains(.,'Environment')]//a</td>
	<td></td>
</tr>
<tr>
	<td>SERVER_DASHBOARD_LINK</td>
	<td>//div[@class='lcs-portal-info']//dl[contains(.,'Server Dashboard')]//a</td>
	<td></td>
</tr>
<tr>
	<td>CLUSTER_NODE_ID</td>
	<td>//div[@class='lcs-cluster-info']//dl[contains(.,'Cluster Node IDs')]/dd</td>
	<td></td>
</tr>
<tr>
	<td>APPLY_TO_ALL_NODES_CHECKBOX</td>
	<td>//input[contains(@id,'applyToSiblingClusterNodesCheckbox')]</td>
	<td></td>
</tr>
<tr>
	<td>CONNECT_BUTTON</td>
	<td>//button[contains(.,'Connect')]</td>
	<td></td>
</tr>
<tr>
	<td>DISCONNECT_BUTTON</td>
	<td>//button[contains(.,'Disconnect')]</td>
	<td></td>
</tr>
<tr>
	<td>RESET_CREDENTIALS_BUTTON</td>
	<td>//button[contains(.,'Reset Credentials')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>