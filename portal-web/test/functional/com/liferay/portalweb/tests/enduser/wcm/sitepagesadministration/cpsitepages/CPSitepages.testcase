<definition component-name="portal-wcm">
	<property name="testray.main.component.name" value="Site Pages Administration" />

	<set-up>
		<execute macro="User#firstLoginPG" />
	</set-up>

	<tear-down>
		<execute macro="Site#tearDownCP" />
	</tear-down>

	<command name="AddPageMultiplePagesToSite" priority="4">
		<execute macro="Page#openURL" />

		<execute macro="Page#addPG">
			<var name="pageName" value="Site Page 1" />
		</execute>

		<execute macro="Page#addPG">
			<var name="pageName" value="Site Page 2" />
		</execute>

		<execute macro="Page#addPG">
			<var name="pageName" value="Site Page 3" />
		</execute>

		<execute macro="SitePage#deleteSitepagesPG" />
	</command>

	<command name="AddPageWithMultipleChildPagesToSite" priority="4">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Public" />
		</execute>

		<for list="Site Child Page 1,Site Child Page 2,Site Child Page 3" param="childPageName" >
			<execute macro="Page#gotoCP">
				<var name="portletName" value="Sites" />
			</execute>

			<execute macro="SitePage#addChildCP">
				<var name="siteChildPageName" value="${childPageName}" />
				<var name="siteName" value="Site Name" />
				<var name="sitePageName" value="Site Page" />
				<var name="sitePagePrivacy" value="Public" />
			</execute>
		</for>

		<execute macro="SitePage#viewChildPG">
			<var name="childPageCount" value="3" />
			<var name="pageName" value="Site Page" />
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="siteURL" value="true" />
		</execute>
	</command>

	<command name="AddSitePrivateChildPage" priority="5">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Private" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addChildCP">
			<var name="siteChildPageName" value="Site Child Page" />
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Private" />
		</execute>

		<execute macro="SitePage#viewChildPG">
			<var name="childPageCount" value="1" />
			<var name="pageAccess" value="Private" />
			<var name="pageName" value="Site Page" />
			<var name="siteChildPageName" value="Site Child Page" />
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePageURL" value="http://localhost:8080/group/site-name/site-child-page" />
			<var name="siteURL" value="true" />
		</execute>
	</command>

	<command name="AddSitePrivatePage" priority="5">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Private" />
		</execute>

		<execute macro="SitePage#viewPG">
			<var name="pageAccess" value="Private" />
			<var name="pageName" value="Site Page" />
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePageURL" value="http://localhost:8080/group/site-name/site-page" />
			<var name="siteURL" value="true" />
		</execute>
	</command>

	<command name="AddSitePrivatePagePortlet" priority="5">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePagePrivacy" value="Private" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Private" />
		</execute>

		<execute macro="Page#addPG">
			<var name="pageAccess" value="Private" />
			<var name="pageName" value="Blogs Page" />
			<var name="siteName" value="Site Name" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="Portlet#addPG">
			<var name="pageAccess" value="Private" />
			<var name="pageName" value="Blogs Page" />
			<var name="portletName" value="Blogs" />
			<var name="siteName" value="Site Name" />
			<var name="siteURL" value="true" />
		</execute>
	</command>

	<command name="AddSitePublicChildPage" priority="5">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Public" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addChildCP">
			<var name="siteChildPageName" value="Site Child Page" />
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Public" />
		</execute>

		<execute macro="SitePage#viewChildPG">
			<var name="childPageCount" value="1" />
			<var name="pageName" value="Site Page" />
			<var name="siteChildPageName" value="Site Child Page" />
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePageURL" value="http://localhost:8080/web/site-name/site-child-page" />
			<var name="siteURL" value="true" />
		</execute>
	</command>

	<command name="AddSitePublicPage" priority="5">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Public" />
		</execute>

		<execute macro="SitePage#viewPG">
			<var name="pageName" value="Site Page" />
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePageURL" value="http://localhost:8080/web/site-name/site-page" />
			<var name="siteURL" value="true" />
		</execute>
	</command>

	<command name="AddSitePublicPagePortlet" priority="5">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Public" />
		</execute>

		<execute macro="Page#addPG">
			<var name="pageName" value="Blogs Page" />
			<var name="siteName" value="Site Name" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Blogs Page" />
			<var name="siteName" value="Site Name" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="Portlet#addPG">
			<var name="portletName" value="Blogs" />
		</execute>
	</command>

	<command name="CopyApplicationsFromPageToPage" priority="4">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Public" />
		</execute>

		<execute macro="Page#addPG">
			<var name="pageName" value="Blog Page" />
			<var name="siteName" value="Site Name" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Blog Page" />
			<var name="siteName" value="Site Name" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="Portlet#addPG">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="SitePage#copyApplicationsFromPageToPagePG">
			<var name="pageName" value="Blog Page" />
			<var name="sitePageName" value="Site Page" />
		</execute>
	</command>

	<command name="DeleteSitePages" priority="4">
		<var name="portletName" value="Site Pages" />
		<var name="siteName" value="Site Name" />
		<var name="sitePageNameList" value="Site Page 1,Site Page 2,Site Page 3" />
		<var name="sitePagePrivacy" value="Public" />

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<for list="Site Page 1,Site Page 2,Site Page 3" param="sitePageName">
			<execute macro="Page#gotoCP">
				<var name="portletName" value="Sites" />
			</execute>

			<execute macro="SitePage#addCP">
				<var name="siteName" value="Site Name" />
				<var name="sitePageName" value="${sitePageName}" />
				<var name="sitePagePrivacy" value="Public" />
			</execute>
		</for>

		<execute macro="Page#gotoPagesCP" >
			<var name="portletName" value="Site Pages" />
			<var name="siteScopeName" value="Site Name" />
		</execute>

		<execute macro="SitePage#deleteSitepagesCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page 2" />
		</execute>
	</command>

	<command name="ViewEditURLLocale" priority="4">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Page#gotoPagesCP">
			<var name="portletName" value="Site Pages" />
			<var name="siteScopeName" value="Site Name" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="sitePageName" value="Language Page" />
			<var name="sitePagePrivacy" value="Public" />
			<var name="siteScopeSelected" value="true" />
		</execute>

		<execute macro="Page#gotoConfigurationCP">
			<var name="portletName" value="Site Settings" />
			<var name="siteScopeName" value="Site Name" />
		</execute>

		<execute macro="Site#configureCurrentLanguagesViaSiteSettings">
			<var name="currentPortalLanguages" value="English (United States),Hungarian (Hungary)" />
			<var name="defaultPortalLanguage" value="English (United States)" />
		</execute>

		<execute macro="User#logoutPG" />

		<execute macro="Page#openURL">
			<var name="specificURL" value="http://localhost:8080/fr/web/site-name/language-page" />
		</execute>

		<execute function="Pause" locator1="2000" />

		<execute function="AssertLocation" value1="http://localhost:8080/en/web/site-name/language-page" />

		<execute macro="Page#openURL">
			<var name="specificURL" value="http://localhost:8080/fr" />
		</execute>

		<execute function="Pause" locator1="2000" />

		<execute function="AssertLocation" value1="http://localhost:8080/en/web/guest" />

		<execute macro="User#loginPG">
			<var name="specificURL" value="http://localhost:8080/fr" />
		</execute>

		<execute function="AssertLocation" value1="http://localhost:8080/web/guest" />

		<execute macro="Page#gotoConfigurationCP">
			<var name="portletName" value="Site Settings" />
			<var name="siteScopeName" value="Site Name" />
		</execute>

		<execute macro="Site#addVirtualHostsURLCP">
			<var name="publicPageVirtualHostURL" value="www.able.com" />
			<var name="publicVirtualHosts" value="true" />
		</execute>

		<execute macro="Page#openURL">
			<var name="virtualHostsURL" value="http://www.able.com:8080/fr/" />
		</execute>

		<execute macro="Site#viewVirtualHostsURLPG">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Language Page" />
			<var name="sitePageURL" value="http://www.able.com:8080/en/" />
		</execute>
	</command>

	<command name="SaveEmbeddedPageType" priority="4">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Public" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Site Page" />
			<var name="siteName" value="Site Name" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="SitePage#embeddedPageTypePG" />
	</command>

	<command name="SavePortletPageType" priority="4">
		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="pageTemplateName" value="Blog" />
			<var name="siteName" value="Site Name" />
			<var name="sitePageName" value="Site Page" />
			<var name="sitePagePrivacy" value="Public" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Site Page" />
			<var name="siteName" value="Site Name" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="BlogsPortlet#viewPG" />
	</command>
</definition>