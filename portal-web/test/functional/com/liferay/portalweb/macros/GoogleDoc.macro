<definition>
	<command name="addCategorization">
		<if>
			<condition function="IsVisible" locator1="PGDocumentsandmediaAddGoogleDocs#CATEGORIZATION_PANEL_COLLAPSED" />
			<then>
				<execute function="AssertClick" locator1="PGDocumentsandmediaAddGoogleDocs#CATEGORIZATION_PANEL_EXPANDED" value1="Categorization" />
			</then>
		</if>

		<execute function="Type" locator1="PGDocumentsandmediaAddGoogleDocs#CATEGORIZATION_TAGS_FIELD" value1="${categoryTag}" />

		<execute function="AssertClick" locator1="PGDocumentsandmediaAddGoogleDocs#CATEGORIZATION_TAGS_ADD_BUTTON" value1="Add" />

		<var name="key_categoryTag" value="${categoryTag}" />

		<execute function="AssertTextEquals" locator1="PGDocumentsandmediaAddGoogleDocs#CATEGORIZATION_TAG" value1="${categoryTag}" />
	</command>

	<command name="publish">
		<execute function="AssertClick" locator1="PGDocumentsandmediaAddGoogleDocs#PUBLISH_BUTTON" value1="Publish" />

		<execute function="AssertTextEquals#assertPartialText" locator1="PGDocumentsandmedia#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="selectDocument">
		<execute function="AssertClick" locator1="PGDocumentsandmediaAddGoogleDocs#SELECT_BUTTON" value1="Select" />

		<execute function="SelectFrame" locator1="GoogleDrive#GOOGLE_DRIVE_IFRAME" />

		<if>
			<condition function="IsVisible" locator1="GoogleDrive#LIST_VIEW_BUTTON" />
			<then>
				<execute function="AssertClick" locator1="GoogleDrive#LIST_VIEW_BUTTON" value1="List view" />
			</then>
		</if>

		<var name="key_googleDocName" value="${googleDocName}" />

		<execute function="AssertClick" locator1="GoogleDrive#LIST_VIEW_DOCUMENT" value1="${googleDocName}" />

		<execute function="AssertClick" locator1="GoogleDrive#SELECT_BUTTON" value1="Select" />

		<execute function="AssertTextEquals" locator1="PGDocumentsandmediaAddGoogleDocs#GOOGLE_DOCS_NAME" value1="${googleDocName}" />
	</command>

	<command name="selectRelatedAsset">
		<if>
			<condition function="IsVisible" locator1="PGDocumentsandmediaAddGoogleDocs#RELATED_ASSETS_PANEL_COLLAPSED" />
			<then>
				<execute function="AssertClick" locator1="PGDocumentsandmediaAddGoogleDocs#RELATED_ASSETS_PANEL_EXPANDED" />
			</then>
		</if>

		<execute function="AssertClick" locator1="PGDocumentsandmediaAddGoogleDocs#RELATED_ASSETS_SELECT_BUTTON" value1="Select" />

		<var name="key_relatedAssetType" value="${relatedAssetType}" />

		<execute function="AssertClick" locator1="PGDocumentsandmediaAddGoogleDocs#RELATED_ASSETS_SELECT_ENTRY" value1="${relatedAssetType}" />

		<execute function="SelectFrame" locator1="RELATED_ASSETS_IFRAME" />

		<var name="key_relatedAssetTitle" value="${relatedAssetTitle}" />

		<execute function="AssertClick" locator1="PGDocumentsandmediaAddGoogleDocs#RELATED_ASSETS_ENTRY_CHOOSE_BUTTON" value1="Choose" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>
</definition>