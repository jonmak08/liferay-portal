<definition>
	<command name="searchNoPG">
		<execute action="PGSearch#type" locator1="SEARCH_FIELD" value1="${searchTerm}" />

		<var name="key_searchIDName" value="Search" />

		<if>
			<equals arg1="${searchSocialOffice}" arg2="true" />
			<then>
				<execute action="PGSearch#keyPress" locator1="SEARCH_FIELD" value1="\RETURN" />
			</then>
			<else>
				<execute action="PGSearch#click" locator1="SEARCH_ICON" />
			</else>
		</if>

		<var name="key_searchAssetTitle" value="${searchAssetTitle}" />

		<execute action="PGSearchResults#assertElementNotPresent" locator1="ASSET_ENTRY_TABLE_TITLE" />
		<execute action="PGSearchResults#assertElementNotPresent" locator1="ASSET_ENTRY_TABLE_TYPE" />

		<if>
			<isset var="searchAssetSummary" />
			<then>
				<execute action="PGSearchResults#assertElementNotPresent" locator1="ASSET_ENTRY_TABLE_SUMMARY" value1="${searchAssetSummary}" />
			</then>
		</if>

		<if>
			<equals arg1="${searchNoResults}" arg2="true" />
			<then>
				<execute action="PGSearchResults#assertTextEquals" locator1="INFO_MESSAGE" value1="No results were found that matched the keywords: ${searchTerm}." />
			</then>
		</if>
	</command>

	<command name="searchPG">
		<execute action="PGSearch#type" locator1="SEARCH_FIELD" value1="${searchTerm}" />

		<if>
			<isset var="localizationIDName" />
			<then>
				<var name="key_searchIDName" value="${localizationIDName}" />
			</then>
			<else>
				<var name="key_searchIDName" value="Search" />
			</else>
		</if>

		<if>
			<equals arg1="${searchSocialOffice}" arg2="true" />
			<then>
				<execute action="PGSearch#keyPress" locator1="SEARCH_FIELD" value1="\RETURN" />
			</then>
			<else>
				<execute action="PGSearch#click" locator1="SEARCH_ICON" />
			</else>
		</if>

		<if>
			<isset var="searchAssetTitle" />
			<then>
				<var name="key_searchAssetTitle" value="${searchAssetTitle}" />

				<execute action="PGSearchResults#assertTextEquals" locator1="ASSET_ENTRY_TABLE_TITLE" value1="${searchAssetTitle}" />
				<execute action="PGSearchResults#assertTextEquals" locator1="ASSET_ENTRY_TABLE_TYPE" value1="${searchAssetType}" />
			</then>
		</if>

		<if>
			<isset var="searchAssetSummary" />
			<then>
				<execute action="PGSearchResults#assertTextEquals" locator1="ASSET_ENTRY_TABLE_SUMMARY" value1="${searchAssetSummary}" />
			</then>
		</if>
	</command>

	<command name="viewSearchNoPG">
		<var name="key_searchAssetTitle" value="${searchAssetTitle}" />

		<execute action="PGSearchResults#assertElementNotPresent" locator1="ASSET_ENTRY_TABLE_TITLE" />
		<execute action="PGSearchResults#assertElementNotPresent" locator1="ASSET_ENTRY_TABLE_TYPE" />

		<if>
			<isset var="searchAssetSummary" />
			<then>
				<execute action="PGSearchResults#assertElementNotPresent" locator1="ASSET_ENTRY_TABLE_SUMMARY" value1="${searchAssetSummary}" />
			</then>
		</if>

		<if>
			<equals arg1="${searchNoResults}" arg2="true" />
			<then>
				<execute action="PGSearchResults#assertTextEquals" locator1="INFO_MESSAGE" value1="No results were found that matched the keywords: ${searchTerm}." />
			</then>
		</if>
	</command>

	<command name="viewSearchPG">
		<var name="key_searchAssetTitle" value="${searchAssetTitle}" />

		<execute action="PGSearchResults#assertTextEquals" locator1="ASSET_ENTRY_TABLE_TITLE" value1="${searchAssetTitle}" />
		<execute action="PGSearchResults#assertTextEquals" locator1="ASSET_ENTRY_TABLE_TYPE" value1="${searchAssetType}" />

		<if>
			<isset var="searchAssetSummary" />
			<then>
				<execute action="PGSearchResults#assertTextEquals" locator1="ASSET_ENTRY_TABLE_SUMMARY" value1="${searchAssetSummary}" />
			</then>
		</if>
	</command>

	<command name="viewSearchResultsPG">
		<if>
			<isset var="searchAssetTitle1" />
			<then>
				<var name="key_searchAssetTitle1" value="${searchAssetTitle1}" />

				<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TITLE_1" value1="${searchAssetTitle1}" />
				<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TYPE_1" value1="${searchAssetType1}" />

				<if>
					<isset var="searchAssetSummary1" />
					<then>
						<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_SUMMARY_1" value1="${searchAssetSummary1}" />
					</then>
				</if>
			</then>
			<elseif>
				<isset var="searchAssetTitle2" />
				<then>
					<var name="key_searchAssetTitle2" value="${searchAssetTitle2}" />

					<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TITLE_2" value1="${searchAssetTitle2}" />
					<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TYPE_2" value1="${searchAssetType2}" />

					<if>
						<isset var="searchAssetSummary2" />
						<then>
							<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_SUMMARY_2" value1="${searchAssetSummary2}" />
						</then>
					</if>
				</then>
			</elseif>
			<elseif>
				<isset var="searchAssetTitle3" />
				<then>
					<var name="key_searchAssetTitle3" value="${searchAssetTitle3}" />

					<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TITLE_3" value1="${searchAssetTitle3}" />
					<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TYPE_3" value1="${searchAssetType3}" />

					<if>
						<isset var="searchAssetSummary3" />
						<then>
							<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_SUMMARY_3" value1="${searchAssetSummary3}" />
						</then>
					</if>
				</then>
			</elseif>
		</if>
	</command>
</definition>