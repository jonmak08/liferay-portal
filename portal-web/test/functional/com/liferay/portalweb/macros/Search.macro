<definition>
	<command name="searchNoPG">
		<execute function="Type" locator1="PGSearch#SEARCH_FIELD" value1="${searchTerm}" />

		<var name="key_searchIDName" value="Search" />

		<if>
			<equals arg1="${searchSocialOffice}" arg2="true" />
			<then>
				<execute function="KeyPress" locator1="PGSearch#SEARCH_FIELD" value1="\RETURN" />
			</then>
			<else>
				<execute function="Click" locator1="PGSearch#SEARCH_ICON" />
			</else>
		</if>

		<var name="key_searchAssetTitle" value="${searchAssetTitle}" />

		<execute function="AssertElementNotPresent" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TITLE" />
		<execute function="AssertElementNotPresent" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TYPE" />

		<if>
			<isset var="searchAssetSummary" />
			<then>
				<execute function="AssertElementNotPresent" locator1="PGSearchResults#ASSET_ENTRY_TABLE_SUMMARY" value1="${searchAssetSummary}" />
			</then>
		</if>

		<if>
			<equals arg1="${searchNoResults}" arg2="true" />
			<then>
				<execute function="AssertTextEquals" locator1="PGSearchResults#INFO_MESSAGE" value1="No results were found that matched the keywords: ${searchTerm}." />
			</then>
		</if>
	</command>

	<command name="searchPG">
		<execute function="Type" locator1="PGSearch#SEARCH_FIELD" value1="${searchTerm}" />

		<if>
			<isset var="localizationIDName" />
			<then>
				<var name="key_searchIDName" value="${localizationIDName}" />
			</then>
			<else>
				<var name="key_searchIDName" value="Search" />
			</else>
		</if>

		<if>
			<equals arg1="${searchSocialOffice}" arg2="true" />
			<then>
				<execute function="KeyPress" locator1="PGSearch#SEARCH_FIELD" value1="\RETURN" />
			</then>
			<else>
				<execute function="Click" locator1="PGSearch#SEARCH_ICON" />
			</else>
		</if>

		<if>
			<isset var="searchAssetTitle" />
			<then>
				<var name="key_searchAssetTitle" value="${searchAssetTitle}" />

				<execute function="AssertTextEquals" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TITLE" value1="${searchAssetTitle}" />
				<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TYPE" value1="${searchAssetType}" />
			</then>
		</if>

		<if>
			<isset var="searchAssetSummary" />
			<then>
				<execute function="AssertTextEquals" locator1="PGSearchResults#ASSET_ENTRY_TABLE_SUMMARY" value1="${searchAssetSummary}" />
			</then>
		</if>
	</command>

	<command name="viewIncrementalSearchPG">
		<execute function="Type" locator1="PGSearch#SEARCH_FIELD" value1="${searchTerm}" />

		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<if>
			<isset var="kbFolderName" />
			<then>
				<execute function="AssertTextEquals" locator1="PGSearch#INCREMENTAL_SEARCH_SUGGESTION" value1="${kbFolderName} > ${kbArticleTitle}" />

				<execute function="AssertClick" locator1="PGSearch#INCREMENTAL_SEARCH_SUGGESTION" value1="${kbFolderName} > ${kbArticleTitle}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="PGSearch#INCREMENTAL_SEARCH_SUGGESTION" value1="${kbArticleTitle}" />

				<execute function="AssertClick" locator1="PGSearch#INCREMENTAL_SEARCH_SUGGESTION" value1="${kbArticleTitle}" />
			</else>
		</if>
	</command>

	<command name="viewSearchNoPG">
		<var name="key_searchAssetTitle" value="${searchAssetTitle}" />

		<execute function="AssertElementNotPresent" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TITLE" />
		<execute function="AssertElementNotPresent" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TYPE" />

		<if>
			<isset var="searchAssetSummary" />
			<then>
				<execute function="AssertElementNotPresent" locator1="PGSearchResults#ASSET_ENTRY_TABLE_SUMMARY" value1="${searchAssetSummary}" />
			</then>
		</if>

		<if>
			<equals arg1="${searchNoResults}" arg2="true" />
			<then>
				<execute function="AssertTextEquals" locator1="PGSearchResults#INFO_MESSAGE" value1="No results were found that matched the keywords: ${searchTerm}." />
			</then>
		</if>
	</command>

	<command name="viewSearchPG">
		<var name="key_searchAssetTitle" value="${searchAssetTitle}" />

		<execute function="AssertTextEquals" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TITLE" value1="${searchAssetTitle}" />
		<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TYPE" value1="${searchAssetType}" />

		<if>
			<isset var="searchAssetSummary" />
			<then>
				<execute function="AssertTextEquals" locator1="PGSearchResults#ASSET_ENTRY_TABLE_SUMMARY" value1="${searchAssetSummary}" />
			</then>
		</if>
	</command>

	<command name="viewSearchResultsPG">
		<if>
			<isset var="searchAssetTitle1" />
			<then>
				<var name="key_searchAssetTitle1" value="${searchAssetTitle1}" />
				<var name="key_searchAssetType1" value="${searchAssetType1}" />

				<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TITLE_1" value1="${searchAssetTitle1}" />
				<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TYPE_1" value1="${searchAssetType1}" />

				<if>
					<isset var="searchAssetSummary1" />
					<then>
						<var name="key_searchAssetSummary1" value="${searchAssetSummary1}" />

						<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_SUMMARY_1" value1="${searchAssetSummary1}" />
					</then>
				</if>
			</then>
		</if>

		<if>
			<isset var="searchAssetTitle2" />
			<then>
				<var name="key_searchAssetTitle2" value="${searchAssetTitle2}" />
				<var name="key_searchAssetType2" value="${searchAssetType2}" />

				<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TITLE_2" value1="${searchAssetTitle2}" />
				<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TYPE_2" value1="${searchAssetType2}" />

				<if>
					<isset var="searchAssetSummary2" />
					<then>
						<var name="key_searchAssetSummary2" value="${searchAssetSummary2}" />

						<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_SUMMARY_2" value1="${searchAssetSummary2}" />
					</then>
				</if>
			</then>
		</if>

		<if>
			<isset var="searchAssetTitle3" />
			<then>
				<var name="key_searchAssetTitle3" value="${searchAssetTitle3}" />
				<var name="key_searchAssetType3" value="${searchAssetType3}" />

				<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TITLE_3" value1="${searchAssetTitle3}" />
				<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_TYPE_3" value1="${searchAssetType3}" />

				<if>
					<isset var="searchAssetSummary3" />
					<then>
						<var name="key_searchAssetSummary3" value="${searchAssetSummary3}" />

						<execute function="AssertTextEquals#assertPartialText" locator1="PGSearchResults#ASSET_ENTRY_TABLE_SUMMARY_3" value1="${searchAssetSummary3}" />
					</then>
				</if>
			</then>
		</if>
	</command>
</definition>