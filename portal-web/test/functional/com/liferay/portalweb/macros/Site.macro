<definition>
	<var name="siteMember" value="User" />
	<var name="siteType" value="Open" />

	<command name="addAllMembersCP">
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#TOOLBAR_ADD_MEMBERS" value1="Add Members" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#ADD_MEMBERS_MENULIST_USER" value1="User" />
		<execute function="Check" locator1="CPSitemembershipsAddmembers#USER_TABLE_ALL_CHECKBOX" />
		<execute function="Click" locator1="CPSitemembershipsAddmembers#SAVE_BUTTON" />
		<execute function="AssertTextEquals#assertText" locator1="CPSitemembershipsAddmembers#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addBlankCP">
		<execute macro="Site#addCP">
			<var name="membershipType" value="${membershipType}" />
			<var name="parentSiteName" value="${parentSiteName}" />
			<var name="siteLimitMembership" value="${siteLimitMembership}" />
			<var name="siteName" value="${siteName}" />
			<var name="siteTemplateName" value="${siteTemplateName}" />
			<var name="siteType" value="Blank" />
			<var name="uncheckPropagation" value="${uncheckPropagation}" />
		</execute>
	</command>

	<command name="addCommunityCP">
		<execute macro="Site#addCP">
			<var name="membershipType" value="${membershipType}" />
			<var name="parentSiteName" value="${parentSiteName}" />
			<var name="siteLimitMembership" value="${siteLimitMembership}" />
			<var name="siteName" value="${siteName}" />
			<var name="siteTemplateName" value="${siteTemplateName}" />
			<var name="siteType" value="Community" />
			<var name="uncheckPropagation" value="${uncheckPropagation}" />
		</execute>
	</command>

	<command name="addCP">
		<execute function="AssertClick#assertTextClickAt" locator1="CPSites#ADD" value1="Add" />

		<if>
			<equals arg1="${siteType}" arg2="Blank" />
			<then>
				<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSites#ADD_BLANK_SITE" value1="Blank Site" />
			</then>
			<elseif>
				<equals arg1="${siteType}" arg2="Community" />
				<then>
					<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSites#ADD_COMMUNITY_SITE" value1="Community Site" />
					<if>
						<equals arg1="${uncheckPropagation}" arg2="true" />
						<then>
							<execute function="Uncheck" locator1="CPSitesAdd#ENABLE_PROPAGATION_CHECKBOX" />
						</then>
					</if>
				</then>
			</elseif>
			<elseif>
				<equals arg1="${siteType}" arg2="Intranet" />
				<then>
					<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSites#ADD_INTRANET_SITE" value1="Intranet Site" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${siteType}" arg2="Site Template" />
				<then>
					<var name="key_siteTemplateName" value="${siteTemplateName}" />

					<execute function="Click" locator1="CPSites#ADD_SITE_TEMPLATE_SITE" />
				</then>
			</elseif>
			<else>
				<fail message="Invalid var siteType. Must be (Blank|Community|Intranet|Site Template)." />
			</else>
		</if>

		<if>
			<equals arg1="${membershipType}" arg2="Private" />
			<then>
				<execute function="Select" locator1="CPSitesAdd#MEMBERSHIP_TYPE" value1="Private" />
			</then>
			<elseif>
				<equals arg1="${membershipType}" arg2="Restricted" />
				<then>
					<execute function="Select" locator1="CPSitesAdd#MEMBERSHIP_TYPE" value1="Restricted" />
				</then>
			</elseif>
			<else>
				<execute function="Select" locator1="CPSitesAdd#MEMBERSHIP_TYPE" value1="Open" />
			</else>
		</if>

		<if>
			<isset var="parentSiteName" />
			<then>
				<execute function="AssertClick" locator1="CPSitesAdd#PARENT_SITE_SELECT" value1="Select" />

				<var name="key_parentSiteName" value="${parentSiteName}" />

				<execute function="SelectFrame" locator1="CPSitesAddSelectSite#SELECT_SITE_IFRAME" />
				<execute function="Type" locator1="CPSitesAddSelectSite#BASIC_SEARCH_FIELD" value1="${parentSiteName}" />
				<execute function="KeyPress" locator1="CPSitesAddSelectSite#BASIC_SEARCH_FIELD" value1="\RETURN" />
				<execute function="AssertTextEquals" locator1="CPSitesAddSelectSite#SITES_TABLE_NAME" value1="${parentSiteName}" />
				<execute function="Click" locator1="CPSitesAddSelectSite#SITES_TABLE_CHOOSE_BUTTON" value1="Choose" />

				<var name="key_parentSiteName" value="${parentSiteName}" />

				<execute function="SelectFrame" value1="relative=top" />
				<execute function="AssertTextEquals" locator1="CPSitesAdd#PARENT_SITE_TABLE_NAME" value1="${parentSiteName}" />
			</then>
		</if>

		<execute function="Type" locator1="CPSitesAdd#DETAILS_NAME" value1="${siteName}" />
		<execute function="AssertClick" locator1="CPSitesAdd#MENU_SAVE" value1="Save" />

		<execute function="AssertTextEquals" locator1="CPSitesEdit#SITE_TITLE" value1="${siteName}" />

		<if>
			<equals arg1="${siteLimitMembership}" arg2="Yes" />
			<then>
				<execute function="Check" locator1="CPSitesEdit#PARENT_SITE_LIMIT_MEMBERSHIP_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${siteLimitMembership}" arg2="No" />
				<then>
					<execute function="Uncheck" locator1="CPSitesEdit#PARENT_SITE_LIMIT_MEMBERSHIP_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<execute function="AssertClick" locator1="CPSitesEdit#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals#assertText" locator1="CPSitesEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addIntranetCP">
		<execute macro="Site#addCP">
			<var name="membershipType" value="${membershipType}" />
			<var name="parentSiteName" value="${parentSiteName}" />
			<var name="siteLimitMembership" value="${siteLimitMembership}" />
			<var name="siteName" value="${siteName}" />
			<var name="siteTemplateName" value="${siteTemplateName}" />
			<var name="siteType" value="Intranet" />
			<var name="uncheckPropagation" value="${uncheckPropagation}" />
		</execute>
	</command>

	<command name="addMemberCP">
		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#TOOLBAR_ADD_MEMBERS" value1="Add Members" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#ADD_MEMBERS_MENULIST_USER" value1="User" />

		<execute function="Type" locator1="CPSitemembershipsAddmembers#SEARCH_FIELD" value1="${userScreenName}" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitemembershipsAddmembers#SEARCH_BUTTON" value1="Search" />

		<if>
			<isset var="userMiddleName" />
			<then>
				<execute function="AssertTextEquals#assertPartialText" locator1="CPSitemembershipsAddmembers#USER_TABLE_NAME" value1="${userFirstName} ${userMiddleName} ${userLastName}" />
			</then>
			<else>
				<execute function="AssertTextEquals#assertPartialText" locator1="CPSitemembershipsAddmembers#USER_TABLE_NAME" value1="${userFirstName} ${userLastName}" />
			</else>
		</if>

		<execute function="AssertTextEquals" locator1="CPSitemembershipsAddmembers#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute function="Check" locator1="CPSitemembershipsAddmembers#USER_TABLE_CHECKBOX" />
		<execute function="Click" locator1="CPSitemembershipsAddmembers#SAVE_BUTTON" />
		<execute function="AssertTextEquals#assertText" locator1="CPSitemembershipsAddmembers#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addMemberOrganizationCP">
		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#TOOLBAR_ADD_MEMBERS" value1="Add Members" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#ADD_MEMBERS_MENULIST_ORGANIZATION" value1="Organization" />
		<execute function="Type" locator1="CPSitemembershipsAddmembers#SEARCH_ORGANIZATION_FIELD" value1="${orgName}" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitemembershipsAddmembers#SEARCH_BUTTON" value1="Search" />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsAddmembers#ORGANIZATION_TABLE_NAME" value1="${orgName}" />
		<execute function="Check" locator1="CPSitemembershipsAddmembers#ORGANIZATION_TABLE_CHECKBOX" />
		<execute function="Click" locator1="CPSitemembershipsAddmembers#SAVE_BUTTON" />
		<execute function="AssertTextEquals#assertText" locator1="CPSitemembershipsAddmembers#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addMemberUserGroupCP">
		<var name="key_userGroupName" value="${userGroupName}" />

		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#TOOLBAR_ADD_MEMBERS" value1="Add Members" />
		<execute function="AssertClick" locator1="CPSitememberships#ADD_MEMBERS_MENULIST_USER_GROUPS" value1="User Group" />
		<execute function="Type" locator1="CPSitemembershipsAddmembers#SEARCH_USER_GROUP_FIELD" value1="${userGroupName}" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitemembershipsAddmembers#SEARCH_BUTTON" value1="Search" />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsAddmembers#USER_GROUP_TABLE_NAME" value1="${userGroupName}" />
		<execute function="Check" locator1="CPSitemembershipsAddmembers#USER_GROUP_TABLE_CHECKBOX" />
		<execute function="Click" locator1="CPSitemembershipsAddmembers#SAVE_BUTTON" />
		<execute function="AssertTextEquals#assertText" locator1="CPSitemembershipsAddmembers#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addSiteTemplateCP">
		<execute macro="Site#addCP">
			<var name="membershipType" value="${membershipType}" />
			<var name="parentSiteName" value="${parentSiteName}" />
			<var name="siteLimitMembership" value="${siteLimitMembership}" />
			<var name="siteName" value="${siteName}" />
			<var name="siteTemplateName" value="${siteTemplateName}" />
			<var name="siteType" value="Site Template" />
			<var name="uncheckPropagation" value="${uncheckPropagation}" />
		</execute>
	</command>

	<command name="addVirtualHostsURLCP">
		<execute function="AssertClick" locator1="CPSitesettings#MENU_BASIC_INFORMATION_SITE_URL" value1="Site URL" />

		<if>
			<isset var="publicVirtualHosts" />
			<then>
				<execute function="AssertTextEquals" locator1="CPSitesettingsSiteURL#SITE_URL_VIRTUAL_HOSTS_PUBLIC_PAGES_LABEL" value1="Public Pages" />
				<execute function="Type" locator1="CPSitesettingsSiteURL#SITE_URL_VIRTUAL_HOSTS_PUBLIC_PAGES_FIELD" value1="${publicPageVirtualHostURL}" />
			</then>
		</if>

		<if>
			<isset var="privateVirtualHosts" />
			<then>
				<execute function="AssertTextEquals" locator1="CPSitesettingsSiteURL#SITE_URL_VIRTUAL_HOSTS_PRIVATE_PAGES_LABEL" value1="Private Pages" />
				<execute function="Type" locator1="CPSitesettingsSiteURL#SITE_URL_VIRTUAL_HOSTS_PRIVATE_PAGES_FIELD" value1="${privatePageVirtualHostURL}" />
			</then>
		</if>

		<execute function="AssertClick#pauseAssertTextClickAt" locator1="CPSitesettingsSiteURL#SAVE_BUTTON" value1="Save" />

		<execute function="AssertTextEquals" locator1="CPSitesettingsSiteURL#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="approveJoinSiteRestrictedViaMySitesPG">
		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="AssertTextEquals" locator1="CPSitememberships#MEMBERSHIP_TYPE" value1="Membership Type: Restricted" />
		<execute function="AssertTextEquals" locator1="CPSitememberships#REQUEST_MESSAGE" value1="There are 1 membership requests pending." />
		<execute function="AssertClick" locator1="CPSitememberships#TOOLBAR_VIEW_MEMBERSHIP_REQUESTS" value1="View Membership Requests" />

		<execute function="AssertTextEquals" locator1="CPSitemembershipsViewmembershiprequestsPending#REQUESTS_TABLE_USER_NAME" value1="${userFirstName} ${userLastName} (${userEmailAddress})" />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsViewmembershiprequestsPending#REQUESTS_TABLE_USER_COMMENTS" value1="Please allow me to join the Restricted Site." />
		<execute function="AssertClick" locator1="CPSitemembershipsViewmembershiprequestsPending#REQUESTS_TABLE_REPLY" value1="Reply" />

		<execute function="Select" locator1="CPSitemembershipsViewmembershiprequestsPendingReply#STATUS" value1="Approve" />
		<execute function="Type" locator1="CPSitemembershipsViewmembershiprequestsPendingReply#REPLY_COMMENTS" value1="You may join the Restricted Site." />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitemembershipsViewmembershiprequestsPendingReply#SAVE_BUTTON" value1="Save" />

		<execute function="AssertTextEquals" locator1="CPSitemembershipsViewmembershiprequestsPending#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsViewmembershiprequestsPending#SUCCESS_MESSAGE_2" value1="Your reply will be sent to the user by email." />
		<execute function="AssertClick" locator1="CPSitemembershipsViewmembershiprequestsPending#VIEW_MEMBERSHIP_NAVIGATION_APPROVED" value1="Approved" />

		<execute function="AssertTextEquals" locator1="CPSitemembershipsViewmembershiprequestsApproved#APPROVED_TABLE_USER_NAME" value1="${userFirstName} ${userLastName} (${userEmailAddress})" />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsViewmembershiprequestsApproved#APPROVED_TABLE_USER_COMMENTS" value1="Please allow me to join the Restricted Site." />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsViewmembershiprequestsApproved#APPROVED_TABLE_REPLIER_NAME" value1="Test Test" />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsViewmembershiprequestsApproved#APPROVED_TABLE_REPLY_COMMENTS" value1="You may join the Restricted Site." />
	</command>

	<command name="assertMemberListNotVisibleCP">
		<for list="${usersEmailsList}" param="userEmail" >
			<var method="StringUtil#replace('${userEmail}', '@liferay.com', '')" name="userName" />

			<execute macro="Site#assertMemberNotVisibleCP">
				<var name="userFirstName" value="${userName}" />
				<var name="userLastName" value="${userName}" />
				<var name="userScreenName" value="${userName}" />
			</execute>
		</for>
	</command>

	<command name="assertMemberNotVisibleCP">
		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="AssertTextNotPresent" locator1="CPSitememberships#USER_TABLE_NAME" value1="${userFirstName} ${userLastName}" />
		<execute function="AssertTextNotPresent" locator1="CPSitememberships#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
	</command>

	<command name="assignRoleCP">
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#TOOLBAR_ADD_SITE_ROLES_TO" value1="Add Site Roles to" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#ADD_SITE_ROLES_TO_MENULIST_USERS" value1="Users" />

		<var name="key_roleName" value="${roleName}" />

		<execute function="Type" locator1="CPSitemembershipsAssignuserroles#ROLES_SEARCH_FIELD" value1="${roleName}" />
		<execute function="AssertClick" locator1="CPSitemembershipsAssignuserroles#ROLES_SEARCH_BUTTON" value1="Search" />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsAssignuserroles#ROLE_TABLE_TITLE" value1="${roleName}" />
		<execute function="Click" locator1="CPSitemembershipsAssignuserroles#ROLE_TABLE_TITLE" value1="${roleName}" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="AssertClick" locator1="CPSitemembershipsAssignuserrolesUser#AVAILABLE" value1="Available" />
		<execute function="Type" locator1="CPSitemembershipsAssignuserrolesUser#BASIC_SEARCH_FIELD" value1="${userScreenName}" />
		<execute function="AssertClick" locator1="CPSitemembershipsAssignuserrolesUser#BASIC_SEARCH_BUTTON" value1="Search" />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsAssignuserrolesUser#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute function="Check" locator1="CPSitemembershipsAssignuserrolesUser#USER_TABLE_CHECKBOX" />
		<execute function="AssertClick" locator1="CPSitemembershipsAssignuserrolesUser#UPDATE_ASSOCIATIONS_BUTTON" value1="Update Associations" />
		<execute function="AssertTextEquals#assertText" locator1="CPSitemembershipsAssignuserrolesUser#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute function="AssertClick" locator1="CPSitemembershipsAssignuserrolesUser#CURRENT" value1="Current" />
		<execute function="AssertChecked" locator1="CPSitemembershipsAssignuserrolesUser#USER_TABLE_CHECKBOX" />

		<if>
			<isset var="userMiddleName" />
			<then>
				<execute function="AssertTextEquals" locator1="CPSitemembershipsAssignuserrolesUser#USER_TABLE_USER_NAME" value1="${userFirstName} ${userMiddleName} ${userLastName}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="CPSitemembershipsAssignuserrolesUser#USER_TABLE_USER_NAME" value1="${userFirstName} ${userLastName}" />
			</else>
		</if>

		<execute function="AssertTextEquals" locator1="CPSitemembershipsAssignuserrolesUser#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
	</command>

	<command name="assignSiteRolesSiteAdministratorUserGroupsCP">
		<var name="key_userGroupName" value="${userGroupName}" />

		<execute function="AssertClick" locator1="CPSitememberships#NAVIGATION_USER_GROUPS" value1="User Groups" />
		<execute function="Type" locator1="CPSitemembershipsUsergroups#SEARCH_FIELD" value1="${userGroupName}" />
		<execute function="AssertClick" locator1="CPSitemembershipsUsergroups#SEARCH_BUTTON" value1="Search" />

		<execute function="AssertTextEquals" locator1="CPSitemembershipsUsergroups#USER_GROUP_TABLE_NAME" value1="${userGroupName}" />
		<execute function="AssertClick" locator1="CPSitemembershipsUsergroups#USER_GROUP_TABLE_ACTIONS" value1="Actions" />
		<execute function="AssertClick" locator1="CPSitemembershipsUsergroups#USER_GROUP_MENULIST_ASSIGN_SITE_ROLES" value1="Assign Site Roles" />

		<execute function="Check" locator1="CPSitemembershipsUsergroups#SITE_ADMINISTRATOR_CHECKBOX" />
		<execute function="Click" locator1="CPSitemembershipsUsergroups#UPDATE_ASSOCIATIONS_BUTTON" />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsUsergroups#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertChecked" locator1="CPSitemembershipsUsergroups#SITE_ADMINISTRATOR_CHECKBOX" />
	</command>

	<command name="assignSiteRolesSiteOwnerUserGroupsCP">
		<var name="key_userGroupName" value="${userGroupName}" />

		<execute function="AssertClick" locator1="CPSitememberships#NAVIGATION_USER_GROUPS" value1="User Groups" />
		<execute function="Type" locator1="CPSitemembershipsUsergroups#SEARCH_FIELD" value1="${userGroupName}" />
		<execute function="AssertClick" locator1="CPSitemembershipsUsergroups#SEARCH_BUTTON" value1="Search" />

		<execute function="AssertTextEquals" locator1="CPSitemembershipsUsergroups#USER_GROUP_TABLE_NAME" value1="${userGroupName}" />
		<execute function="AssertClick" locator1="CPSitemembershipsUsergroups#USER_GROUP_TABLE_ACTIONS" value1="Actions" />
		<execute function="AssertClick" locator1="CPSitemembershipsUsergroups#USER_GROUP_MENULIST_ASSIGN_SITE_ROLES" value1="Assign Site Roles" />

		<execute function="Check" locator1="CPSitemembershipsUsergroups#SITE_OWNER_CHECKBOX" />
		<execute function="Click" locator1="CPSitemembershipsUsergroups#UPDATE_ASSOCIATIONS_BUTTON" />
		<execute function="AssertTextEquals" locator1="CPSitemembershipsUsergroups#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertChecked" locator1="CPSitemembershipsUsergroups#SITE_OWNER_CHECKBOX" />
	</command>

	<command name="assignUserSitesCP">
		<execute function="AssertClick" locator1="CPUsersandorganizations#SEARCH_ALL_USERS" value1="All Users" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="Type" locator1="CPUsersandorganizationsViewusers#BASIC_SEARCH_FIELD" value1="${userScreenName}" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPUsersandorganizationsViewusers#BASIC_SEARCH_BUTTON" value1="Search" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsViewusers#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsViewusers#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsUser#MENU_USER_INFORMATION_SITES" value1="Sites" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsUser#SITES_SELECT_LINK" value1="Select" />
		<execute function="SelectFrame" locator1="CPUsersandorganizationsUser#SITES_SELECT_IFRAME" />

		<var name="key_siteName" value="${siteName}" />

		<execute function="Type" locator1="CPUsersandorganizationsUser#SITES_SELECT_SEARCH_FIELD" value1="${siteName}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsUser#SITES_SELECT_SEARCH_BUTTON" value1="Search" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsUser#SITES_SELECT_TABLE_NAME" value1="${siteName}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsUser#SITES_SELECT_CHOOSE_BUTTON" value1="Choose" />
		<execute function="SelectFrame" value1="relative=top" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPUsersandorganizationsUser#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsUser#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="configureCurrentLanguagesViaSiteSettings">
		<execute function="AssertClick" locator1="CPSitesettings#MENU_MISCELLANEOUS_DISPLAY_SETTINGS" value1="Display Settings" />

		<execute function="Click" locator1="CPSitesettingsDisplaysettings#DEFINE_DEFAULT_AND_AVAILABLE_SITE_LANGUAGES_RADIO" />

		<if>
			<isset var="defaultSiteLanguage" />
			<then>
				<var name="defaultSiteLanguage" value="${defaultSiteLanguage}" />

				<execute function="Select" locator1="CPSitesettingsDisplaysettings#DEFAULT_LANGUAGE_SELECT" value1="${defaultSiteLanguage}" />
			</then>
		</if>

		<if>
			<isset var="currentSiteLanguages" />
			<then>
				<while>
					<condition function="IsElementPresent" locator1="CPSitesettingsDisplaysettings#AVAILABLE_LANGUAGES_CURRENT_PRESENT" />
					<then>
						<execute function="Click" locator1="CPSitesettingsDisplaysettings#AVAILABLE_LANGUAGES_CURRENT_PRESENT" />
						<execute function="Click" locator1="CPSitesettingsDisplaysettings#MOVE_CURRENT_TO_AVAILABLE_BUTTON" />
					</then>
				</while>

				<for list="${currentSiteLanguages}" param="currentSiteLanguage">
					<var name="currentSiteLanguage" value="${currentSiteLanguage}" />

					<execute function="Select" locator1="CPSitesettingsDisplaysettings#AVAILABLE_LANGUAGES_AVAILABLE_SELECT" value1="${currentSiteLanguage}" />
					<execute function="Click" locator1="CPSitesettingsDisplaysettings#MOVE_AVAILABLE_TO_CURRENT_BUTTON" />
					<execute function="AssertTextEquals#assertPartialText" locator1="CPSitesettingsDisplaysettings#AVAILABLE_LANGUAGES_CURRENT_SELECT" value1="${currentSiteLanguage}" />
				</for>
			</then>
		</if>

		<execute function="AssertClick" locator1="CPPortalsettingsDisplaysettings#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPPortalsettingsDisplaysettings#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="deactivateCP">
		<var name="key_siteName" value="${siteName}" />

