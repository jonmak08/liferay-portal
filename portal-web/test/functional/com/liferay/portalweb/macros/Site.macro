		<execute function="AssertTextEquals" locator1="CPSites#SITE_TABLE_TYPE" value1="${siteType}" />

		<if>
			<equals arg1="${siteMember}" arg2="Organization" />
			<then>
				<execute function="AssertTextEquals" locator1="CPSites#SITE_TABLE_MEMBERS_ORGS" value1="1 ${siteMember}" />
			</then>
			<elseif>
				<equals arg1="${siteMember}" arg2="User" />
				<then>
					<execute function="AssertTextEquals" locator1="CPSites#SITE_TABLE_MEMBERS_USERS" value1="1 ${siteMember}" />
				</then>
			</elseif>
			<else>
				<fail message="Invalid var siteMember. Must be (Organization|User)." />
			</else>
		</if>

		<execute function="AssertTextEquals" locator1="CPSites#SITE_TABLE_ACTIVE" value1="Yes" />
		<execute function="AssertTextEquals" locator1="CPSites#SITE_TABLE_PENDING_REQUESTS" value1="" />
		<execute function="AssertTextEquals" locator1="CPSites#SITE_TABLE_TAGS" value1="" />
		<execute function="AssertTextEquals#assertText" locator1="CPSites#SITE_TABLE_ACTIONS" value1="Actions" />
	</command>

	<command name="viewLimitMembershipParentSiteCP">
		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#TOOLBAR_ADD_MEMBERS" value1="Add Members" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSitememberships#ADD_MEMBERS_MENULIST_USER" value1="User" />
		<execute function="AssertTextNotPresent" locator1="CPSitemembershipsAddmembers#USER_TABLE_NAME" value1="${userFirstName} ${userLastName}" />
		<execute function="AssertTextNotPresent" locator1="CPSitemembershipsAddmembers#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
	</command>

	<command name="viewMemberCP">
		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="AssertTextEquals#assertPartialText" locator1="CPSitememberships#USER_TABLE_NAME" value1="${userFirstName} ${userLastName}" />
		<execute function="AssertTextEquals" locator1="CPSitememberships#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute function="AssertTextEquals" locator1="CPSitememberships#USER_TABLE_ACTIONS" value1="Actions" />
	</command>

	<command name="viewMemberListCP">
		<for list="${usersEmailsList}" param="userEmail" >
			<var method="StringUtil#replace('${userEmail}', '@liferay.com', '')" name="userName" />

			<execute macro="Site#viewMemberCP">
				<var name="userFirstName" value="${userName}" />
				<var name="userLastName" value="${userName}" />
				<var name="userScreenName" value="${userName}" />
			</execute>
		</for>
	</command>

	<command name="viewMemberOrganizationCP">
		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPSitememberships#NAVIGATION_ORGANIZATIONS" value1="Organizations" />

		<execute function="AssertTextEquals#assertPartialText" locator1="CPSitememberships#ORGANIZATION_TABLE_NAME" value1="${orgName}" />
		<if>
			<equals arg1="${toolTip}" arg2="true" />
			<then>
				<execute function="AssertTextEquals#assertPartialText" locator1="CPSitememberships#ORGANIZATION_TABLE_TOOLTIP" value1="This site belongs to ${orgName} which is an organization" />
			</then>
		</if>
	</command>

	<command name="viewMySitesViaMySitesPG">
		<var name="key_siteName" value="${siteName}" />

		<if>
			<equals arg1="${organizationSite}" arg2="true" />
			<then>
				<execute function="AssertClick" locator1="Dockbar#MY_SITES" value1="My Sites" />
				<execute function="AssertTextNotPresent" locator1="Dockbar#MY_SITES_PUBLIC_SITE" value1="${siteName}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="PGMysites#NAVIGATION_MY_SITES" value1="My Sites" />
				<execute function="AssertTextEquals" locator1="PGMysites#SITE_TABLE_SITE_NAME" value1="${siteName}" />
				<execute function="AssertTextEquals" locator1="PGMysites#SITE_TABLE_SITE_NAME_MEMBERS" value1="2" />
				<execute function="AssertTextEquals" locator1="PGMysites#SITE_TABLE_SITE_ACTIONS" value1="Actions" />
			</else>
		</if>
	</command>

	<command name="viewSiteAdminPermissionsCP">
		<var name="key_siteName" value="${siteName}" />

		<execute function="Type" locator1="CPSites#BASIC_SEARCH_FIELD" value1="${siteName}" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPSites#BASIC_SEARCH_BUTTON" value1="Search" />
		<execute function="AssertClick" locator1="CPSites#SITE_TABLE_ACTIONS" value1="Actions" />
		<execute function="AssertTextEquals" locator1="CPSites#SITE_MENULIST_SITE_ADMINISTRATION" value1="Site Administration" />
		<execute function="AssertTextEquals" locator1="CPSites#SITE_MENULIST_ADD_CHILD_SITE" value1="Add Child Site" />
		<execute function="AssertTextEquals" locator1="CPSites#SITE_MENULIST_LEAVE" value1="Leave" />
		<execute function="AssertTextEquals" locator1="CPSites#SITE_MENULIST_DEACTIVATE" value1="Deactivate" />
		<execute function="AssertTextEquals" locator1="CPSites#SITE_MENULIST_DELETE" value1="Delete" />
	</command>

	<command name="viewSiteFriendlyURLPG">
		<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_1" value1="${siteName}" />
		<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_2" value1="${sitePageName}" />
		<execute function="AssertTextEquals" locator1="Home#SITE_NAME" value1="${siteName}" />
		<execute function="AssertLocation" value1="${sitePageURL}" />
	</command>

	<command name="viewSitePG">
		<execute macro="Page#gotoPG">
			<var name="defaultURL" value="${defaultURL}" />
			<var name="nodePort" value="${nodePort}" />
			<var name="pageName" value="${sitePageName}" />
			<var name="soDashboard" value="${soDashboard}" />
			<var name="soPage" value="${soPage}" />
		</execute>

		<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_1" value1="${siteName}" />
		<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_2" value1="${childSiteName}" />
		<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_3" value1="${sitePageName}" />
		<execute function="AssertTextEquals" locator1="Home#SITE_NAME" value1="${childSiteName}" />
		<execute function="AssertLocation" value1="${sitePageURL}" />
	</command>

	<command name="viewVirtualHostsURLPG">
		<var name="key_pageName" value="${sitePageName}" />

		<execute function="AssertTextEquals" locator1="Home#SITE_NAME" value1="${siteName}" />
		<execute function="AssertTextEquals" locator1="Home#PAGE" value1="${sitePageName}" />
		<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_1" value1="${siteName}" />
		<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_2" value1="${sitePageName}" />

		<execute function="AssertLocation" value1="${sitePageURL}" />
	</command>
</definition>