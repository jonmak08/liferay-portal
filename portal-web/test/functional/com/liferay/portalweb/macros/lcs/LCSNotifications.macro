<definition>
	<command name="addRuleAllNotifications">
		<execute macro="LCSNotificationsNavigator#gotoAddRule" />

		<execute macro="LCSNotificationsRule#selectRuleScope">
			<var name="environmentName" value="${environmentName}" />
			<var name="projectName" value="${projectName}" />
			<var name="serverName" value="${serverName}" />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="Monitoring is unavailable." />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="New Liferay Connected Services Client is available." />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="New fix pack is available." />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="New patching tool is available." />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="The patching tool is unavailable." />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="The server is offline." />
		</execute>

		<execute macro="LCSNotificationsNavigator#saveRule" />
	</command>

	<command name="addRuleOneNotification">
		<execute macro="LCSNotificationsNavigator#gotoAddRule" />

		<execute macro="LCSNotificationsRule#selectRuleScope">
			<var name="environmentName" value="${environmentName}" />
			<var name="projectName" value="${projectName}" />
			<var name="serverName" value="${serverName}" />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="New fix pack is available." />
		</execute>

		<execute macro="LCSNotificationsNavigator#saveRule" />
	</command>

	<command name="deleteRule">
		<var name="key_environmentName" value="${environmentName}" />
		<var name="key_projectName" value="${projectName}" />
		<var name="key_serverName" value="${serverName}" />

		<execute function="AssertClick" locator1="LCSNotifications#NOTIFICATION_TABLE_RULE_ACTIONS_BUTTON" value1="Actions" />

		<execute function="AssertClickNoError" locator1="LCSNotifications#ACTIONS_MENU_DELETE" value1="Delete" />

		<execute function="Confirm" value1="Are you sure you want to delete notifications rule?" />

		<execute function="AssertTextEquals" locator1="LCSNotifications#NOTIFICATIONS_ALERT_SUCCESS" value1="Your request completed successfully." />
	</command>

	<command name="editRuleAllNotifications">
		<execute macro="LCSNotificationsNavigator#gotoEditRule">
			<var name="environmentName" value="${environmentName}" />
			<var name="projectName" value="${projectName}" />
			<var name="serverName" value="${serverName}" />
		</execute>

		<execute macro="LCSNotificationsRule#viewRuleScope">
			<var name="environmentName" value="${environmentName}" />
			<var name="projectName" value="${projectName}" />
			<var name="serverName" value="${serverName}" />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="Monitoring is unavailable." />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="New Liferay Connected Services Client is available." />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="New fix pack is available." />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="New patching tool is available." />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="The patching tool is unavailable." />
		</execute>

		<execute macro="LCSNotificationsRule#checkRuleNotification">
			<var name="notificationRule" value="The server is offline." />
		</execute>

		<execute macro="LCSNotificationsNavigator#saveRule" />
	</command>

	<command name="viewRuleAllNotifications">
		<execute macro="LCSNotificationsRule#viewRuleNotificationsTable">
			<var name="environmentName" value="${environmentName}" />
			<var name="projectName" value="${projectName}" />
			<var name="serverName" value="${serverName}" />
		</execute>

		<execute macro="LCSNotificationsNavigator#gotoEditRule">
			<var name="environmentName" value="${environmentName}" />
			<var name="projectName" value="${projectName}" />
			<var name="serverName" value="${serverName}" />
		</execute>

		<execute macro="LCSNotificationsRule#viewRuleScope">
			<var name="environmentName" value="${environmentName}" />
			<var name="projectName" value="${projectName}" />
			<var name="serverName" value="${serverName}" />
		</execute>

		<execute macro="LCSNotificationsRule#viewRuleNotificationChecked">
			<var name="notificationRule" value="Monitoring is unavailable." />
		</execute>

		<execute macro="LCSNotificationsRule#viewRuleNotificationChecked">
			<var name="notificationRule" value="New Liferay Connected Services Client is available." />
		</execute>

		<execute macro="LCSNotificationsRule#viewRuleNotificationChecked">
			<var name="notificationRule" value="New fix pack is available." />
		</execute>

		<execute macro="LCSNotificationsRule#viewRuleNotificationChecked">
			<var name="notificationRule" value="New patching tool is available." />
		</execute>

		<execute macro="LCSNotificationsRule#viewRuleNotificationChecked">
			<var name="notificationRule" value="The patching tool is unavailable." />
		</execute>

		<execute macro="LCSNotificationsRule#viewRuleNotificationChecked">
			<var name="notificationRule" value="The server is offline." />
		</execute>
	</command>
</definition>