<definition>
	<command name="gotoDefaultTab">
		<var name="key_metricsTabName" value="${metricsTabName}" />

		<execute function="AssertClick" locator1="LCSServerMetrics#NAVIGATION_DEFAULT_TAB" value1="${metricsTabName}" />
	</command>

	<command name="gotoSubTab">
		<var name="key_metricsSubTabName" value="${metricsTabName}" />

		<execute function="AssertClick" locator1="LCSServerMetrics#NAVIGATION_SUB_TAB" value1="${metricsSubTabName}" />
	</command>
</definition>