<definition>
	<command name="viewServerDetails">
		<var name="key_environmentName" value="${environmentName}" />

		<execute function="AssertTextEquals" locator1="LCSPortalNavigation#NAVIGATION_MENU_ENVIRONMENT" value1="${environmentName}" />

		<var name="key_serverName" value="${serverName}" />

		<execute function="AssertTextEquals#assertPartialText" locator1="LCSPortalNavigation#NAVIGATION_MENU_SERVER" value1="${serverName}" />
		<execute function="AssertTextEquals" locator1="LCSServerServerDetails#SERVER_NAME" value1="Name ${serverName}" />
		<execute function="AssertTextEquals" locator1="LCSServerServerDetails#SERVER_DESCRIPTION" value1="Description ${serverDescription}" />
		<execute function="AssertTextEquals" locator1="LCSServerServerDetails#SERVER_LOCATION" value1="Location ${serverLocation}" />
	</command>
</definition>