<definition>
	<command name="addCommentPG">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmedia#assertClick" locator1="ENTRY_ICON_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmediaDocument#assertClick" locator1="COMMENTS_BE_THE_FIRST" value1="Be the first." />
		<execute action="PGDocumentsandmediaDocument#type" locator1="COMMENTS_COMMENT_FIELD" value1="${commentBody}" />

		<if>
			<condition action="PGDocumentsandmediaDocument#isElementPresent" locator1="COMMENTS_SUBMIT_BUTTON" value1="Submit" />
			<then>
				<execute action="PGDocumentsandmediaDocument#assertClick" locator1="COMMENTS_SUBMIT_BUTTON" value1="Submit" />
			</then>
			<else>
				<execute action="PGDocumentsandmediaDocument#assertClick" locator1="COMMENTS_REPLY_BUTTON" value1="Reply" />
			</else>
		</if>

		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request processed successfully." />
	</command>

	<command name="addCP">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="CPDocumentsandmedia#assertClick" locator1="TOOLBAR_ADD" value1="Add" />
		<execute action="CPDocumentsandmedia#assertClick" locator1="ADD_MENULIST_BASIC_DOCUMENT" value1="Basic Document" />

		<if>
			<equals arg1="${uploadTempFile}" arg2="true" />
			<then>
				<execute action="CPDocumentsandmediaAdddocument#uploadTempFile" locator1="FILE_FIELD" value1="${dmDocumentFile}" />
			</then>
			<elseif>
				<equals arg1="${nullFile}" arg2="true" />
				<then>
					<execute action="CPDocumentsandmediaAdddocument#assertElementPresent" locator1="FILE_FIELD" />
				</then>
			</elseif>
			<else>
				<execute action="CPDocumentsandmediaAdddocument#uploadCommonFile" locator1="FILE_FIELD" value1="${dmDocumentFile}" />
			</else>
		</if>

		<execute action="CPDocumentsandmediaAdddocument#type" locator1="TITLE_FIELD" value1="${dmDocumentTitle}" />

		<if>
			<isset var="dmDocumentDescription" />
			<then>
				<execute action="CPDocumentsandmediaAdddocument#type" locator1="DESCRIPTION_FIELD" value1="${dmDocumentDescription}" />
			</then>
		</if>

		<execute action="CPDocumentsandmediaAdddocument#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />

		<execute action="CPDocumentsandmedia#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="CPDocumentsandmedia#assertTextEquals" locator1="DOCUMENT_CONTAINER_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />
	</command>

	<command name="addMultipleDocumentsPG">
		<execute action="PGDocumentsandmedia#assertClick" locator1="TOOLBAR_ADD" value1="Add" />
		<execute action="PGDocumentsandmedia#assertClick" locator1="ADD_MENULIST_MULTIPLE_DOCUMENTS" value1="Multiple Documents" />
	</command>

	<command name="addPG">
		<if>
			<equals arg1="${stagingActivated}" arg2="true" />
			<then>
				<execute action="PGDocumentsandmedia#assertElementNotPresent" locator1="STAGING_MESSAGE" value1="The data of this portlet is not staged. Any data changes are immediately available to the Local Live site. The portlet's own workflow is still honored. Portlet setup is still managed from staging." />
			</then>
		</if>

		<execute action="PGDocumentsandmedia#assertClick" locator1="TOOLBAR_ADD" value1="Add" />

		<if>
			<isset var="dmDocumentTypeName" />
			<then>
				<var name="key_dmDocumentTypeName" value="${dmDocumentTypeName}" />

				<execute action="PGDocumentsandmedia#assertClick" locator1="ADD_MENULIST_DOCUMENT_TYPE" value1="${dmDocumentTypeName}" />
			</then>
			<else>
				<execute action="PGDocumentsandmedia#assertClick" locator1="ADD_MENULIST_BASIC_DOCUMENT" value1="Basic Document" />
			</else>
		</if>

		<execute action="PGDocumentsandmediaAdddocument#uploadCommonFile" locator1="FILE_FIELD" value1="${dmDocumentFile}" />
		<execute action="PGDocumentsandmediaAdddocument#type" locator1="TITLE_FIELD" value1="${dmDocumentTitle}" />
		<execute action="PGDocumentsandmediaAdddocument#type" locator1="DESCRIPTION_FIELD" value1="${dmDocumentDescription}" />

		<if>
			<isset var="categoryName" />
			<then>
				<var name="key_categoryName" value="${categoryName}" />

				<execute macro="PGDocumentsandmediaEditdocument#showCategorization" />
				<execute action="PGDocumentsandmediaEditdocument#assertClick" locator1="CATEGORIZATION_TOPIC_SELECT_BUTTON" value1="Select" />

				<execute action="PGDocumentsandmediaCategoriesDialogBox#assertClick" locator1="CATEGORY_TOPIC" value1="${categoryName}" />
				<execute action="PGDocumentsandmediaCategoriesDialogBox#assertClick" locator1="CLOSE_BUTTON" value1="×" />

				<execute action="PGDocumentsandmediaEditdocument#assertTextEquals" locator1="CATEGORIZATION_VOCABULARY_TAG" value1="${categoryName}" />
			</then>
		</if>

		<if>
			<isset var="tagNameList" />
			<then>
				<for list="${tagNameList}" param="tagName">
					<execute macro="PGDocumentsandmediaEditdocument#showCategorization" />

					<execute action="PGDocumentsandmediaEditdocument#type" locator1="CATEGORIZATION_TAGS_FIELD" value1="${tagName}" />
					<execute action="PGDocumentsandmediaEditdocument#assertClick" locator1="CATEGORIZATION_TAGS_ADD_BUTTON" value1="Add" />
				</for>
			</then>
		</if>

		<execute action="PGDocumentsandmediaAdddocument#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />

		<if>
			<equals arg1="${dmDocument}" arg2="Duplicate" />
			<then>
				<execute action="PGDocumentsandmediaAdddocument#assertTextEquals" locator1="ERROR_MESSAGE" value1="Your request failed to complete." />
				<execute action="PGDocumentsandmediaAdddocument#assertTextEquals" locator1="UNIQUE_DOCUMENT_ERROR_MESSAGE" value1="Please enter a unique document name." />
			</then>
			<elseif>
				<equals arg1="${dmDocument}" arg2="Invalid" />
				<then>
					<execute action="PGDocumentsandmediaAdddocument#assertTextEquals" locator1="ERROR_MESSAGE_1" value1="Your request failed to complete." />
					<execute action="PGDocumentsandmediaAdddocument#assertTextEquals" locator1="ERROR_MESSAGE_2" value1="Please enter a file with a valid file name." />
				</then>
			</elseif>
			<else>
				<execute action="PGDocumentsandmedia#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</else>
		</if>
	</command>

	<command name="addPGViaFlashUploader">
		<if>
			<equals arg1="${stagingActivated}" arg2="true" />
			<then>
				<execute action="PGDocumentsandmedia#assertElementNotPresent" locator1="STAGING_MESSAGE" value1="The data of this portlet is not staged. Any data changes are immediately available to the Local Live site. The portlet's own workflow is still honored. Portlet setup is still managed from staging." />
			</then>
		</if>

		<execute action="PGDocumentsandmedia#assertClick" locator1="TOOLBAR_ADD" value1="Add" />
		<execute action="PGDocumentsandmedia#assertClick" locator1="ADD_MENULIST_MULTIPLE_DOCUMENTS" value1="Multiple Documents" />

		<if>
			<isset var="dmDocumentFileList" />
			<then>
				<for list="${dmDocumentFileList}" param="dmDocumentFile">
					<var name="key_dmDocumentFile" value="${dmDocumentFile}" />

					<execute action="PGDocumentsandmediaAddmultipledocuments#assertClick" locator1="SELECT_FILES_BUTTON" value1="Select Files" />
					<execute function="#pause" locator1="15000" />

					<if>
						<equals arg1="${uploadTempFile}" arg2="true" />
						<then>
							<execute action="OS#sikuliUploadTempFile" locator1="FILE_DIALOG_BOX_FILE_NAME_FIELD_PNG" value1="${dmDocumentFile}" />
						</then>
						<else>
							<execute action="OS#sikuliUploadCommonFile" locator1="FILE_DIALOG_BOX_FILE_NAME_FIELD_PNG" value1="${dmDocumentFile}" />
						</else>
					</if>

					<execute action="PGDocumentsandmediaAddmultipledocuments#assertChecked" locator1="ALL_FILES_READY_TO_BE_SAVED_SPECIFIC_CHECKBOX" />
					<execute action="PGDocumentsandmediaAddmultipledocuments#assertTextEquals" locator1="ALL_FILES_READY_TO_BE_SAVED_TITLE" value1="${dmDocumentFile}" />
				</for>
			</then>
			<else>
				<var name="key_dmDocumentFile" value="${dmDocumentFile}" />

				<execute action="PGDocumentsandmediaAddmultipledocuments#assertClick" locator1="SELECT_FILES_BUTTON" value1="Select Files" />

				<if>
					<equals arg1="${uploadTempFile}" arg2="true" />
					<then>
						<execute action="OS#sikuliUploadTempFile" locator1="FILE_DIALOG_BOX_FILE_NAME_FIELD_PNG" value1="${dmDocumentFile}" />
					</then>
					<else>
						<execute action="OS#sikuliUploadCommonFile" locator1="FILE_DIALOG_BOX_FILE_NAME_FIELD_PNG" value1="${dmDocumentFile}" />
					</else>
				</if>

				<execute action="PGDocumentsandmediaAddmultipledocuments#assertChecked" locator1="ALL_FILES_READY_TO_BE_SAVED_SELECT_ALL_CHECKBOX" />
				<execute action="PGDocumentsandmediaAddmultipledocuments#assertTextEquals" locator1="ALL_FILES_READY_TO_BE_SAVED_TITLE" value1="${dmDocumentFile}" />
			</else>
		</if>

		<if>
			<isset var="categoryName" />
			<then>
				<var name="key_categoryName" value="${categoryName}" />
				<if>
					<condition action="PGDocumentsandmediaAddmultipledocuments#isVisible" locator1="CATEGORIZATION_DROPDOWN_COLLAPSED" />
					<then>
						<execute action="PGDocumentsandmediaAddmultipledocuments#assertClick" locator1="CATEGORIZATION_DROPDOWN_COLLAPSED" value1="Categorization" />
					</then>
				</if>

				<execute action="PGDocumentsandmediaAddmultipledocuments#assertClick" locator1="CATEGORIZATION_TOPIC_SELECT_BUTTON" value1="Select" />

				<execute action="PGDocumentsandmediaCategoriesDialogBox#assertClick" locator1="CATEGORY_TOPIC" value1="${categoryName}" />
				<execute action="PGDocumentsandmediaCategoriesDialogBox#assertClick" locator1="CLOSE_BUTTON" value1="×" />

				<execute action="PGDocumentsandmediaAddmultipledocuments#assertTextEquals" locator1="CATEGORIZATION_VOCABULARY_TAG" value1="${categoryName}" />
			</then>
		</if>

		<if>
			<isset var="categoryTag" />
			<then>
				<execute action="PGDocumentsandmediaAddmultipledocuments#assertClick" locator1="TAGS_ENTRY_BOX" />
				<execute action="PGDocumentsandmediaAddmultipledocuments#type" locator1="TAGS_ENTRY_BOX" value1="${categoryTag}" />
				<execute action="PGDocumentsandmediaAddmultipledocuments#assertClick" locator1="TAGS_ADD_BUTTON" value1="Add" />
				<execute action="PGDocumentsandmediaAddmultipledocuments#assertTextEquals" locator1="CATEGORIZATION_TAG" value1="${categoryTag}" />
			</then>
		</if>

		<execute action="PGDocumentsandmediaAddmultipledocuments#assertClick" locator1="SAVE_BUTTON" value1="Save" />
		<execute action="PGDocumentsandmediaAddmultipledocuments#assertTextEquals" locator1="ALL_FILES_READY_TO_BE_SAVED_SUCCESS_MESSAGE" value1="Successfully saved." />
	</command>

	<command name="addPGViaMG">
		<execute action="PGMediagalleryFolder#assertClick" locator1="FOLDER_MENU_ADD_MEDIA" value1="Add Media" />

		<execute action="PGMediagallerySelectmediatype#selectFrame" locator1="SELECT_MEDIA_TYPE_IFRAME" />
		<execute action="PGMediagallerySelectmediatype#assertClick" locator1="MEDIA_TABLE_BASIC_DOCUMENT" value1="Basic Document" />
		<execute action="PGMediagallerySelectmediatype#selectFrameTop" value1="relative=top" />

		<execute action="PGMediagalleryAddmedia#uploadCommonFile" locator1="FILE_FIELD" value1="${mgDocumentFile}" />
		<execute action="PGMediagalleryAddmedia#type" locator1="TITLE_FIELD" value1="${mgDocumentTitle}" />
		<execute action="PGMediagalleryAddmedia#type" locator1="DESCRIPTION_FIELD" value1="${mgDocumentDescription}" />
		<execute action="PGMediagalleryAddmedia#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />

		<execute action="PGMediagallery#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addShortcutPG">
		<execute action="PGDocumentsandmedia#assertClick" locator1="TOOLBAR_ADD" value1="Add" />
		<execute action="PGDocumentsandmedia#assertClick" locator1="ADD_MENULIST_SHORTCUT" value1="Shortcut" />

		<execute action="PGDocumentsandmediaAddshortcut#assertClick" locator1="SITE_SELECT_BUTTON" value1="Select" />

		<execute action="PGDocumentsandmediaAddshortcut#selectFrame" locator1="SELECT_SITE_IFRAME" />

		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />
		<var name="key_siteName" value="${siteName}" />

		<execute action="PGDocumentsandmediaAddshortcut#type" locator1="SELECT_SITE_SEARCH_FIELD" value1="${siteName}" />
		<execute action="PGDocumentsandmediaAddshortcut#assertClick" locator1="SELECT_SITE_SEARCH_BUTTON" value1="Search" />
		<execute action="PGDocumentsandmediaAddshortcut#assertTextEquals" locator1="SELECT_SITE_TABLE_NAME" value1="${siteName}" />
		<execute action="PGDocumentsandmediaAddshortcut#assertClick" locator1="SELECT_SITE_TABLE_CHOOSE_BUTTON" value1="Choose" />
		<execute action="PGDocumentsandmediaAddshortcut#selectFrameTop" value1="relative=top" />
		<execute action="PGDocumentsandmediaAddshortcut#assertClick" locator1="DOCUMENT_SELECT_BUTTON" value1="Select" />
		<execute action="PGDocumentsandmediaAddshortcut#selectFrame" locator1="SELECT_FILE_IFRAME" />

		<if>
			<isset var="dmFolderName" />
			<then>
				<var name="key_dmFolderName" value="${dmFolderName}" />

				<execute action="PGDocumentsandmediaAddshortcut#assertClick" locator1="SELECT_FOLDER_TABLE_FOLDER" value1="${dmFolderName}" />
			</then>
		</if>

		<execute action="PGDocumentsandmediaAddshortcut#assertTextEquals" locator1="SELECT_FILE_DOCUMENT" value1="${dmDocumentTitle} ${dmDocumentDescription}" />
		<execute action="PGDocumentsandmediaAddshortcut#assertClick" locator1="SELECT_FILE_CHOOSE_BUTTON" value1="Choose" />
		<execute action="PGDocumentsandmediaAddshortcut#selectFrameTop" value1="relative=top" />
		<execute action="PGDocumentsandmediaAddshortcut#assertClick" locator1="SAVE_BUTTON" value1="Save" />

		<execute action="PGDocumentsandmedia#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="cancelCheckoutPG">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmedia#assertClick" locator1="ENTRY_ICON_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="CHECKOUT_SUCCESS_MESSAGE" value1="You now have a lock on this document. No one else can edit this document until you unlock it. This lock will automatically expire in 1 day." />

		<execute action="PGDocumentsandmediaDocument#assertClick" locator1="TOOLBAR_CANCEL_CHECKOUT_BUTTON" value1="Cancel Checkout" />
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="cannotViewCP">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="CPDocumentsandmedia#assertElementNotPresent" locator1="DOCUMENT_CONTAINER_DOCUMENT" />
	</command>

	<command name="cannotViewDocumentMetadataCP">
		<var name="key_metadataData" value="${metadataData}" />
		<var name="key_metadataLabel" value="${metadataLabel}" />

		<execute action="CPDocumentsandmediaDocument#assertElementNotPresent" locator1="DOCUMENT_EXTRACTED_METADATA_FIELD" />
	</command>

	<command name="checkDocumentCheckboxCP">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="CPDocumentsandmedia#check" locator1="DOCUMENT_CONTAINER_DOCUMENT_CHECKBOX" />
	</command>

	<command name="checkinCP">
		<if>
			<equals arg1="${sharepointRepository}" arg2="true" />
			<then>
				<execute action="CPDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE_DOCUMENT_VIEW" value1="You now have an indefinite lock on this document. No one else can edit this document until you unlock it. This lock will never expire." />
			</then>
			<else>
				<execute action="CPDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE_DOCUMENT_VIEW" value1="You now have a lock on this document. No one else can edit this document until you unlock it. This lock will automatically expire in 1 day." />
			</else>
		</if>

		<execute action="CPDocumentsandmediaDocument#assertClick" locator1="TOOLBAR_CHECKIN_BUTTON" value1="Checkin" />

		<execute action="CPDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="checkinPG">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmedia#assertClick" locator1="ENTRY_ICON_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="CHECKOUT_SUCCESS_MESSAGE" value1="You now have a lock on this document. No one else can edit this document until you unlock it. This lock will automatically expire in 1 day." />
		<execute action="PGDocumentsandmediaDocument#assertClick" locator1="TOOLBAR_CHECKIN_BUTTON" value1="Checkin" />

		<if>
			<condition action="PGDocumentsandmediaCheckin#isElementPresent" locator1="MAJOR_REVISION" />
			<then>
				<if>
					<equals arg1="${revision}" arg2="Major" />
					<then>
						<execute action="PGDocumentsandmediaCheckin#click" locator1="MAJOR_REVISION" />
					</then>
					<elseif>
						<equals arg1="${revision}" arg2="Minor" />
						<then>
							<execute action="PGDocumentsandmediaCheckin#click" locator1="MINOR_REVISION" />
						</then>
					</elseif>
				</if>

				<if>
					<isset var="changeLog" />
					<then>
						<execute action="PGDocumentsandmediaCheckin#type" locator1="CHANGE_LOG" value1="${changeLog}" />
					</then>
				</if>

				<execute action="PGDocumentsandmediaCheckin#assertClick" locator1="SAVE_AND_CHECKIN_BUTTON" value1="Save and Checkin" />
			</then>
		</if>

		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="checkinViaDocumentActionsPG">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmedia#click" locator1="ENTRY_ICON_DOCUMENT_ACTIONS" />
		<execute action="PGDocumentsandmedia#assertClick" locator1="DOCUMENT_MENULIST_CHECKIN" value1="Checkin" />

		<if>
			<condition action="PGDocumentsandmediaCheckin#isElementPresent" locator1="MAJOR_REVISION" />
			<then>
				<if>
					<equals arg1="${revision}" arg2="Major" />
					<then>
						<execute action="PGDocumentsandmediaCheckin#click" locator1="MAJOR_REVISION" />
					</then>
					<elseif>
						<equals arg1="${revision}" arg2="Minor" />
						<then>
							<execute action="PGDocumentsandmediaCheckin#click" locator1="MINOR_REVISION" />
						</then>
					</elseif>
				</if>

				<if>
					<isset var="changeLog" />
					<then>
						<execute action="PGDocumentsandmediaCheckin#type" locator1="CHANGE_LOG" value1="${changeLog}" />
					</then>
				</if>

				<execute action="PGDocumentsandmediaCheckin#assertClick" locator1="SAVE_AND_CHECKIN_BUTTON" value1="Save and Checkin" />
			</then>
		</if>

		<execute action="PGDocumentsandmedia#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="checkoutCP">
		<execute action="CPDocumentsandmediaDocument#assertClick" locator1="TOOLBAR_CHECKOUT_BUTTON" value1="Checkout" />
		<execute action="CPDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<if>
			<equals arg1="${sharepointRepository}" arg2="true" />
			<then>
				<execute action="CPDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE_DOCUMENT_VIEW" value1="You now have an indefinite lock on this document. No one else can edit this document until you unlock it. This lock will never expire." />
			</then>
			<else>
				<execute action="CPDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE_DOCUMENT_VIEW" value1="You now have a lock on this document. No one else can edit this document until you unlock it. This lock will automatically expire in 1 day." />
			</else>
		</if>
	</command>

	<command name="checkoutPG">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmedia#assertClick" locator1="ENTRY_ICON_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmediaDocument#assertClick" locator1="TOOLBAR_CHECKOUT_BUTTON" value1="Checkout" />
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="CHECKOUT_SUCCESS_MESSAGE" value1="You now have a lock on this document. No one else can edit this document until you unlock it. This lock will automatically expire in 1 day." />
	</command>

	<command name="checkoutViaDocumentActionsPG">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmedia#click" locator1="ENTRY_ICON_DOCUMENT_ACTIONS" />
		<execute action="PGDocumentsandmedia#assertClick" locator1="DOCUMENT_MENULIST_CHECKOUT" value1="Checkout" />
		<execute action="PGDocumentsandmedia#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="deleteCommentPG">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmedia#assertClick" locator1="ENTRY_ICON_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmediaDocument#makeVisible" locator1="COMMENTS_DELETE" value1="Delete" />
		<execute action="PGDocumentsandmediaDocument#assertClickNoError" locator1="COMMENTS_DELETE_LINK" value1="Delete" />
		<execute action="PGDocumentsandmediaDocument#confirm" locator1="CONFIRMATION" value1="Are you sure you want to delete the selected comment?" />
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request processed successfully." />
	</command>

	<command name="deleteCP">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="CPDocumentsandmedia#click" locator1="DOCUMENT_CONTAINER_DOCUMENT_DROPDOWN" />

		<if>
			<isset var="recycleBinDisabled" />
			<then>
				<execute action="CPDocumentsandmedia#assertClickNoError" locator1="DOCUMENT_MENULIST_DELETE" value1="Delete" />
				<execute action="CPDocumentsandmedia#confirm" locator1="CONFIRMATION" value1="Are you sure you want to delete this? It will be deleted immediately." />
				<execute action="CPDocumentsandmedia#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</then>
			<else>
				<execute action="CPDocumentsandmedia#assertClick" locator1="DOCUMENT_MENULIST_RECYCLE_BIN" value1="Move to the Recycle Bin" />
				<execute action="PGDocumentsandmedia#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="moved to the Recycle Bin. Undo" />
			</else>
		</if>
	</command>

	<command name="deletePG">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="PGDocumentsandmedia#assertClick" locator1="ENTRY_ICON_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />

		<execute macro="DMDocument#viewDocumentTitleCmdPG">
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
		</execute>

		<execute action="PGDocumentsandmediaDocument#assertClick" locator1="TOOLBAR_MOVE_TO_RECYCLE_BIN_BUTTON" value1="Move to the Recycle Bin" />

		<execute action="PGDocumentsandmedia#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="moved to the Recycle Bin. Undo" />
	</command>

	<command name="deletePGViaMG">
		<var name="key_mgDocumentTitle" value="${mgDocumentTitle}" />

		<execute action="PGMediagalleryFolder#assertClick" locator1="ENTRY_TITLE" value1="${mgDocumentTitle}" />
		<execute action="PGMediagalleryMedia#click" locator1="MOVE_TO_THE_RECYCLE_BIN" />
		<execute action="PGMediagalleryFolder#assertTextEquals" locator1="RECYCLE_BIN_SUCCESS_MESSAGE" value1="moved to the Recycle Bin. Undo" />
		<execute action="PGMediagalleryFolder#assertElementNotPresent" locator1="ENTRY_TITLE" value1="${mgDocumentTitle}" />
	</command>

	<command name="deleteViaActionsPG">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

