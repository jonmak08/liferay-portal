		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="DOCUMENT_DETAILS_VERSION" value1="Version ${dmDocumentVersionNumber}" />
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="DOCUMENT_DETAILS_UPLOADER" value1="Last Updated by ${dmDocumentUploader}" />
		<execute action="PGDocumentsandmediaDocument#assertElementPresent" locator1="DOCUMENT_DETAILS_DATE" />
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="DOCUMENT_DETAILS_WORKFLOW" value1="Status: ${workflow}" />
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="DOCUMENT_DETAILS_DOWNLOAD" value1="Download (${dmDocumentSize})" />
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="DOCUMENT_DETAILS_URL" value1="Get URL or WebDAV URL." />
	</command>

	<command name="viewPGNonAuthor">
		<execute macro="DMDocument#viewPG">
			<var name="dmDocumentDescription" value="${dmDocumentDescription}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
		</execute>

		<if>
			<condition action="SODocuments#isElementPresent" locator1="DOCUMENT_BREADCRUMBS" />
			<then>
				<execute action="SODocuments#assertElementPresent" locator1="DOCUMENT_BREADCRUMBS" />
			</then>
			<else>
				<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="BACK_BUTTON" value1="Back" />
			</else>
		</if>

		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="TOOLBAR_DOWNLOAD_BUTTON" value1="Download" />

		<execute macro="DMDocument#viewDocumentAuthorCmdPG">
			<var name="dmDocumentAuthor" value="${dmDocumentAuthor}" />
		</execute>

		<execute macro="DMDocument#viewDocumentNoCommentsCmdPG" />

		<if>
			<isset var="dmDocumentVersionNumber" />
			<then>
				<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="DOCUMENT_INFO_VERSION" value1="Version ${dmDocumentVersionNumber}" />
			</then>
			<else>
				<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="DOCUMENT_INFO_VERSION" value1="Version 1.0" />
			</else>
		</if>

		<execute macro="DMDocument#viewDocumentDetailsCmdPG" />
	</command>

	<command name="viewPGRating">
		<execute action="PGDocumentsandmediaDocument#assertTextEquals" locator1="AVERAGE_RATING" value1="Average (1 Vote)" />

		<execute action="PGDocumentsandmediaDocument#assertElementPresent" locator1="AVERAGE_RATING_STAR_1" />
		<execute action="PGDocumentsandmediaDocument#assertElementNotPresent" locator1="AVERAGE_RATING_STAR_2" />
		<execute action="PGDocumentsandmediaDocument#assertElementNotPresent" locator1="AVERAGE_RATING_STAR_3" />
		<execute action="PGDocumentsandmediaDocument#assertElementNotPresent" locator1="AVERAGE_RATING_STAR_4" />
		<execute action="PGDocumentsandmediaDocument#assertElementNotPresent" locator1="AVERAGE_RATING_STAR_5" />
		<execute action="PGDocumentsandmediaDocument#assertElementPresent" locator1="AVERAGE_RATING_STAR_EMPTY_1" />
		<execute action="PGDocumentsandmediaDocument#assertElementPresent" locator1="AVERAGE_RATING_STAR_EMPTY_2" />
		<execute action="PGDocumentsandmediaDocument#assertElementPresent" locator1="AVERAGE_RATING_STAR_EMPTY_3" />
		<execute action="PGDocumentsandmediaDocument#assertElementPresent" locator1="AVERAGE_RATING_STAR_EMPTY_4" />
	</command>

	<command name="viewPGViaMG">
		<var name="key_mgDocumentTitle" value="${mgDocumentTitle}" />

		<execute action="PGMediagalleryFolder#assertElementPresent" locator1="ENTRY_THUMBNAIL" />
		<execute action="PGMediagalleryFolder#assertElementPresent" locator1="ENTRY_TITLE" value1="${mgDocumentTitle}" />
	</command>

	<command name="viewPGviaStagingLocalLive">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<if>
			<equals arg1="${documentAdded}" arg2="true" />
			<then>
				<execute action="PGDocumentsandmedia#assertTextEquals" locator1="ENTRY_ICON_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />
				<execute action="PGDocumentsandmedia#assertClick" locator1="LIVE_BUTTON" value1="Live" />

				<execute action="PGDocumentsandmedia#assertTextNotPresent" locator1="ENTRY_ICON_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />
				<execute action="PGDocumentsandmedia#assertTextEquals" locator1="INFO_MESSAGE" value1="There are no documents or media files in this folder." />
			</then>
			<elseif>
				<equals arg1="${documentAdded}" arg2="false" />
				<then>
					<execute action="PGDocumentsandmedia#assertTextEquals" locator1="INFO_MESSAGE" value1="There are no documents or media files in this folder." />

					<execute action="PGDocumentsandmedia#assertClick" locator1="LIVE_BUTTON" value1="Live" />
					<execute action="PGDocumentsandmedia#assertTextEquals" locator1="INFO_MESSAGE" value1="There are no documents or media files in this folder." />
				</then>
			</elseif>
		</if>
	</command>

	<command name="viewSearchResultsCP">
		<var name="key_dmRepositoryName" value="${dmRepositoryName}" />

		<if>
			<isset var="dmRepositoryName" />
			<then>
				<execute action="CPDocumentsandmediaSearch#assertClick" locator1="NAVIGATION_REPOSITORY" value1="${dmRepositoryName}" />
			</then>
		</if>

		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute action="CPDocumentsandmediaSearch#assertTextEquals" locator1="SEARCH_RESULTS_TITLE" value1="${dmDocumentTitle}" />
		<execute action="CPDocumentsandmediaSearch#assertTextEquals" locator1="SEARCH_RESULTS_PARENT_FOLDER" value1="${dmParentFolder}" />
	</command>
</definition>