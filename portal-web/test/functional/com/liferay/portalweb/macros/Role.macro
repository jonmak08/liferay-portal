					<execute function="#type" locator1="CPRolesEdit#DESCRIPTION_FIELD" value1="${roleDescriptionLocalized}" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${language}" arg2="Hebrew (Israel)" />
				<then>
					<execute function="#click" locator1="CPRolesEdit#DESCRIPTION_FIELD_LOCALIZATION_FLAG_HEBREW" />
					<execute function="#type" locator1="CPRolesEdit#DESCRIPTION_FIELD" value1="${roleDescriptionLocalized}" />
				</then>
			</elseif>
			<elseif>
