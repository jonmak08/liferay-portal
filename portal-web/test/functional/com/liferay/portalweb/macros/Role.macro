	<command name="removePermissionCP">
		<var name="key_roleName" value="${roleName}" />

		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPRoles#SEARCH_BUTTON" value1="Search" />
		<execute function="#assertTextEquals" locator1="CPRoles#ROLE_TABLE_TITLE" value1="${roleName}" />
		<execute function="#assertClick" locator1="CPRoles#ROLE_TABLE_ACTIONS" value1="Actions" />
		<execute function="#assertClick" locator1="CPRoles#ROLE_MENULIST_DEFINE_PERMISSIONS" value1="Define Permissions" />

		<execute function="#type" locator1="CPRolesDefinepermissions#SEARCH_FIELD" value1="${permissionNavigationValue}" />
		<execute function="#assertClick" locator1="CPRolesDefinepermissions#${permissionNavigationKey}" value1="${permissionNavigationValue}" />

		<execute function="#uncheck" locator1="CPRolesDefinepermissionsAllpermissions#${permissionDefinitionKey}" />
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPRolesDefinepermissionsAllpermissions#SAVE_BUTTON" value1="Save" />

		<var name="key_permissionDefinitionValue" value="${permissionDefinitionValue}" />
		<var name="key_permissionNavigationValue" value="${permissionNavigationValue}" />

		<execute function="#assertTextEquals" locator1="CPRolesDefinepermissions#SUCCESS_MESSAGE" value1="The role permissions were updated." />
		<execute function="#assertElementNotPresent" locator1="CPRolesDefinepermissions#PERMISSION_TABLE_PERMISSIONS" value1="${permissionNavigationValue}" />
		<execute function="#assertElementNotPresent" locator1="CPRolesDefinepermissions#PERMISSION_TABLE_PERMISSIONS" value1="${permissionDefinitionValue}" />
		<execute function="#assertElementNotPresent" locator1="CPRolesDefinepermissions#PERMISSION_TABLE_REMOVE" value1="Remove" />
