<definition>
	<command name="addCategoryLocalizationCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#ADD_CATEGORY_BUTTON" value1="Add Category" />

		<execute function="#type" locator1="CPCategoriesAdd#NAME_FIELD" value1="${categoryName}" />

		<var name="key_localizationName" value="${localizationNameKey}" />

		<execute function="#click" locator1="CPCategoriesAdd#LOCALIZATION_NAME" />
		<execute function="#type" locator1="CPCategoriesAdd#NAME_FIELD" value1="${localizationName}" />

		<if>
			<isset var="localization2NameKey" />
			<then>
				<var name="key_localizationName" value="${localization2NameKey}" />

				<execute function="#click" locator1="CPCategoriesAdd#LOCALIZATION_NAME" />
				<execute function="#type" locator1="CPCategoriesAdd#NAME_FIELD" value1="${localization2Name}" />
			</then>
		</if>

		<execute function="#assertClick" locator1="CPCategoriesAdd#SAVE_BUTTON" value1="Save" />

		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
		<execute function="#assertTextEquals" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
	</command>

	<command name="addCategoryPropertyCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#ADD_CATEGORY_BUTTON" value1="Add Category" />

		<execute function="#type" locator1="CPCategoriesAdd#NAME_FIELD" value1="${categoryName}" />

		<if>
			<condition function="#isElementPresent" locator1="CPCategoriesAdd#PROPERTIES_BAR_COLLAPSED" />
			<then>
				<execute function="#click" locator1="CPCategoriesAdd#PROPERTIES_BAR" />
			</then>
		</if>

		<var name="i" value="0" />
		<var name="pathNumber" value="0" />

		<if>
			<equals arg1="${propertyNumber}" arg2="0" />
			<then>
				<var name="key_pathNumber" value="${pathNumber}" />

				<execute function="#type" locator1="CPCategoriesAdd#PROPERTIES_KEY_FIELD" value1="${propertiesKeyField} Key" />
				<execute function="#type" locator1="CPCategoriesAdd#PROPERTIES_VALUE_FIELD" value1="${propertiesValueField} Value" />
				<execute function="#click" locator1="CPCategoriesAdd#PROPERTIES_ADD_ROW_BUTTON" />
			</then>
			<else>
				<while>
					<not>
						<equals arg1="${i}" arg2="${propertyNumber}" />
					</not>
					<then>
						<var name="i" value="${i?increment()}" />
						<var name="key_pathNumber" value="${pathNumber}" />
						<var name="pathNumber" value="${pathNumber?increment()}" />
						<var name="pathNumber" value="${pathNumber?increment()}" />

						<execute function="#type" locator1="CPCategoriesAdd#PROPERTIES_KEY_FIELD" value1="${propertiesKeyField} ${i} Key" />
						<execute function="#type" locator1="CPCategoriesAdd#PROPERTIES_VALUE_FIELD" value1="${propertiesValueField} ${i} Value" />
						<execute function="#click" locator1="CPCategoriesAdd#PROPERTIES_ADD_ROW_BUTTON" />
					</then>
				</while>
			</else>
		</if>

		<execute function="#assertClick" locator1="CPCategoriesAdd#SAVE_BUTTON" value1="Save" />

		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
		<execute function="#assertTextEquals" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
	</command>

	<command name="addCategoryToVocabularyCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#ADD_CATEGORY_BUTTON" value1="Add Category" />

		<execute function="#type" locator1="CPCategoriesAdd#NAME_FIELD" value1="${categoryName}" />
		<execute function="#select" locator1="CPCategoriesAdd#VOCABULARY_SELECT" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategoriesAdd#SAVE_BUTTON" value1="Save" />

		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
		<execute function="#assertTextEquals" locator1="CPCategories#INFO_MESSAGE" value1="There are no categories." />
	</command>

	<command name="addCategoryViewableByCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#ADD_CATEGORY_BUTTON" value1="Add Category" />

		<execute function="#type" locator1="CPCategoriesAdd#NAME_FIELD" value1="${categoryName}" />
		<execute function="#select" locator1="CPCategoriesAdd#VIEWABLE_BY_SELECT" value1="${viewableBy}" />
		<execute function="#assertClick" locator1="CPCategoriesAdd#SAVE_BUTTON" value1="Save" />

		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
		<execute function="#assertTextEquals" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
	</command>

	<command name="addCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#ADD_CATEGORY_BUTTON" value1="Add Category" />

		<execute function="#type" locator1="CPCategoriesAdd#NAME_FIELD" value1="${categoryName}" />

		<if>
			<isset var="categoryDescription" />
			<then>
				<execute function="#type" locator1="CPCategoriesAdd#DESCRIPTION_FIELD" value1="${localizationName}" />
			</then>
		</if>

		<execute function="#assertClick" locator1="CPCategoriesAdd#SAVE_BUTTON" value1="Save" />

		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
		<execute function="#assertTextEquals" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
	</command>

	<command name="addWithNullTitleCP">
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#ADD_CATEGORY_BUTTON" value1="Add Category" />

		<execute function="#type" locator1="CPCategoriesAdd#NAME_FIELD" value1=" " />
		<execute function="#assertClick" locator1="CPCategoriesAdd#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="CPCategoriesAdd#ERROR_MESSAGE" value1="Please enter a valid category name." />
	</command>

	<command name="deleteByActionsCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertTextEquals" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
		<execute function="#check" locator1="CPCategories#ALL_CATEGORIES_CHECKBOX" />
		<execute function="#assertClick" locator1="CPCategories#ACTIONS_BUTTON" value1="Actions" />
		<execute function="#assertClickNoError" locator1="CPCategories#ACTIONS_MENULIST_DELETE" value1="Delete" />
		<execute function="#confirm" locator1="CPCategories#CONFIRMATION" value1="Are you sure you want to delete the selected categories?" />
		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
		<execute function="#assertTextEquals" locator1="CPCategories#INFO_MESSAGE" value1="There are no categories." />
		<execute function="#assertTextNotPresent" value1="${categoryName}" />
	</command>

	<command name="deleteByDetailsCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
		<execute function="#assertClickNoError" locator1="CPCategories#CATEGORY_DETAILS_DELETE_BUTTON" value1="Delete" />
		<execute function="#confirm" locator1="CPCategories#CONFIRMATION" value1="Are you sure you want to delete this category?" />
		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
		<execute function="#assertTextEquals" locator1="CPCategories#INFO_MESSAGE" value1="There are no categories." />
		<execute function="#assertTextNotPresent" value1="${categoryName}" />
	</command>

	<command name="deleteCategoriesCP">
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#check" locator1="CPCategories#ALL_CATEGORIES_CHECKBOX" />
		<execute function="#assertClick" locator1="CPCategories#ACTIONS_BUTTON" value1="Actions" />
		<execute function="#assertClickNoError" locator1="CPCategories#ACTIONS_MENULIST_DELETE" value1="Delete" />
		<execute function="#confirm" locator1="CPCategories#CONFIRMATION" value1="Are you sure you want to delete the selected categories?" />
		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
		<execute function="#assertTextEquals" locator1="CPCategories#INFO_MESSAGE" value1="There are no categories." />
	</command>

	<command name="deleteCategoryProperty2CP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
		<execute function="#assertClick" locator1="CPCategories#CATEGORY_DETAILS_EDIT_BUTTON" value1="Edit" />

		<execute function="#click" locator1="CPCategoriesEdit#PROPERTIES_DELETE_ROW_BUTTON_2" />
		<execute function="#assertElementPresent" locator1="CPCategoriesEdit#PROPERTIES_UNDO_MESSAGE" />
		<execute function="#assertClick" locator1="CPCategoriesEdit#SAVE_BUTTON" value1="Save" />

		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
	</command>

	<command name="editCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
		<execute function="#assertClick" locator1="CPCategories#CATEGORY_DETAILS_EDIT_BUTTON" value1="Edit" />

		<execute function="#type" locator1="CPCategoriesEdit#NAME_FIELD" value1="${categoryNameEdit}" />
		<execute function="#assertClick" locator1="CPCategoriesEdit#SAVE_BUTTON" value1="Save" />

		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />

		<var name="key_categoryName" value="${categoryNameEdit}" />

		<execute function="#assertTextEquals" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryNameEdit}" />
	</command>

	<command name="editVocabularyDraganddropCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="Vocabulary Name 1" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="Vocabulary Name 1" />
		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />

		<var name="key_vocabularyName" value="Vocabulary Name 2" />

		<execute function="#dragAndDrop" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" locator2="CPCategories#VOCABULARY_ENTRY" value1="Category Name" />
		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
		<execute function="#assertTextEquals" locator1="CPCategories#VOCABULARY_ENTRY" value1="Vocabulary Name 2" />
		<execute function="#assertTextEquals" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
	</command>

	<command name="gotoCategoryPG">
		<var name="key_categoryName" value="${categoryName}" />

		<execute function="#assertClick" locator1="PGCategoriesnavigation#CATEGORY_NAME" value1="${key_categoryName}" />
	</command>

	<command name="moveViaEditCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
		<execute function="#assertClick" locator1="CPCategories#CATEGORY_DETAILS_EDIT_BUTTON" value1="Edit" />

		<execute function="#select" locator1="CPCategoriesEdit#TO_VOCABULARY_SELECT" value1="${vocabularyName2}" />
		<execute function="#assertClick" locator1="CPCategoriesEdit#SAVE_BUTTON" value1="Save" />

		<execute function="#assertTextEquals" locator1="CPCategories#SUCCESS_MESSAGE" value1="Your request processed successfully." />
	</command>

	<command name="viewCategoryPropertyCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<var name="i" value="0" />

		<if>
			<equals arg1="${propertyNumber}" arg2="0" />
			<then>
				<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
				<execute function="#assertClick" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
				<execute function="#assertTextEquals" locator1="CPCategories#CATEGORY_DETAILS_PROPERTY_KEY" value1="${propertiesKeyField} Key" />
				<execute function="#assertTextEquals" locator1="CPCategories#CATEGORY_DETAILS_PROPERTY_VALUE" value1="${propertiesValueField} Value" />
			</then>
			<else>
				<while>
					<not>
						<equals arg1="${i}" arg2="${propertyNumber}" />
					</not>
					<then>
						<var name="i" value="${i?increment()}" />

						<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
						<execute function="#assertClick" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
						<execute function="#assertTextEquals" locator1="CPCategories#CATEGORY_DETAILS_PROPERTY_KEY_2" value1="${propertiesKeyField} ${i} Key" />
						<execute function="#assertTextEquals" locator1="CPCategories#CATEGORY_DETAILS_PROPERTY_VALUE_2" value1="${propertiesValueField} ${i} Value" />
					</then>
				</while>
			</else>
		</if>
	</command>

	<command name="viewCategoryToVocabularyCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
		<execute function="#assertTextEquals" locator1="CPCategories#CATEGORY_DETAILS_HEADER" value1="${categoryName}" />
	</command>

	<command name="viewCategoryViewableByCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
		<execute function="#assertClick" locator1="CPCategories#CATEGORY_DETAILS_PERMISSIONS_BUTTON" value1="Permissions" />

		<execute function="#selectFrame" locator1="CPCategoriesEditPermissions#IFRAME" />

		<if>
			<equals arg1="${viewableBy}" arg2="Anyone (Guest Role)" />
			<then>
				<execute function="#assertChecked" locator1="CPCategoriesEditPermissions#GUEST_VIEW_CHECKBOX" />
				<execute function="#assertChecked" locator1="CPCategoriesEditPermissions#OWNER_VIEW_CHECKBOX" />
				<execute function="#assertChecked" locator1="CPCategoriesEditPermissions#SITE_MEMBER_VIEW_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${viewableBy}" arg2="Owner" />
				<then>
					<execute function="#assertChecked" locator1="CPCategoriesEditPermissions#OWNER_VIEW_CHECKBOX" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${viewableBy}" arg2="Site Members" />
				<then>
					<execute function="#assertChecked" locator1="CPCategoriesEditPermissions#SITE_MEMBER_VIEW_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<execute function="#selectFrameTop" value1="relative=top" />
	</command>

	<command name="viewCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertTextEquals" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
	</command>

	<command name="viewFilterViaCategoriesNavigationPG">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertTextEquals" locator1="PGCategoriesnavigation#VOCABULARY_NAME" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="PGCategoriesnavigation#CATEGORY_NAME" value1="${categoryName}" />

		<execute function="#assertTextEquals" locator1="PGAssetpublisher#CATEGORIES_FILTER_CATEGORY" value1="${categoryName}" />
	</command>

	<command name="viewMoveViaEditCP">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName}" />
		<execute function="#assertTextEquals" locator1="CPCategories#INFO_MESSAGE" value1="There are no categories." />

		<var name="key_vocabularyName" value="${vocabularyName2}" />

		<execute function="#assertClick" locator1="CPCategories#VOCABULARY_ENTRY" value1="${vocabularyName2}" />
		<execute function="#assertTextEquals" locator1="CPCategories#VOCABULARY_CATEGORY_LABEL" value1="${categoryName}" />
	</command>

	<command name="viewRemoveFilterViaCategoriesNavigationPG">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertTextEquals" locator1="PGCategoriesnavigation#VOCABULARY_NAME" value1="${vocabularyName}" />
		<execute function="#assertClick" locator1="PGCategoriesnavigation#CATEGORY_NAME" value1="${categoryName}" />

		<execute function="#assertElementNotPresent" locator1="PGAssetpublisher#CATEGORIES_FILTER_CATEGORY" value1="${categoryName}" />
	</command>
</definition>