<definition>
	<command name="addCategoryPGViaWCD">
		<execute action="PGWebcontentdisplay#click" locator1="EDIT_WEB_CONTENT" value1="Edit" />

		<execute action="PGWebcontentdisplayEditwebcontent#assertClick" locator1="MENU_CATEGORIZATION" value1="Categorization" />

		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute action="PGWebcontentdisplayEditwebcontentCategorization#assertClick" locator1="CATEGORIZATION_SELECT_BUTTON" value1="Select" />
		<execute action="PGWebcontentdisplayEditwebcontentCategorization#assertClick" locator1="CATEGORIES_CATEGORY_UNSELECTED" value1="${categoryName}" />
		<execute action="PGWebcontentdisplayEditwebcontentCategorization#assertElementPresent" locator1="CATEGORIES_CATEGORY_SELECTED" value1="${categoryName}" />
		<execute action="PGWebcontentdisplayEditwebcontentCategorization#click" locator1="CATEGORIES_CLOSE_BUTTON" />
		<execute action="PGWebcontentdisplayEditwebcontentCategorization#assertTextEquals" locator1="CATEGORIZATION_CATEGORY" value1="${categoryName}" />
		<execute action="PGWebcontentdisplayEditwebcontentCategorization#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />
	</command>

	<command name="addCP">
		<execute action="CPWebcontent#assertClick" locator1="TOOLBAR_ADD" value1="Add" />
		<execute action="CPWebcontent#assertClick" locator1="ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<if>
			<isset var="expirationDate" />
			<then>
				<execute action="CPWebcontentAddwebcontent#assertClick" locator1="MENU_SCHEDULE" value1="Schedule" />

				<execute action="CPWebcontentEditwebcontentSchedule#assertChecked" locator1="SCHEDULE_EXPIRATION_DATE_NEVER_EXPIRE_CHECKBOX" value1="Never Expire" />
				<execute action="CPWebcontentEditwebcontentSchedule#uncheck" locator1="SCHEDULE_EXPIRATION_DATE_NEVER_EXPIRE_CHECKBOX" value1="Never Expire" />
				<execute action="CPWebcontentEditwebcontentSchedule#type" locator1="SCHEDULE_EXPIRATION_DATE_FIELD" value1="${expirationDate}" />
				<execute action="CPWebcontentEditwebcontentSchedule#assertClick" locator1="MENU_CONTENT" value1="Content" />
			</then>
		</if>

		<execute action="CPWebcontentAddwebcontent#type" locator1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="CPWebcontentAddwebcontent#type" locator1="CONTENT_FIELD" value1="${webContentContent}" />
		<execute action="CPWebcontentAddwebcontent#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />

		<execute action="CPWebcontent#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute action="CPWebcontent#click" locator1="TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute action="CPWebcontent#assertClick" locator1="DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />
		<execute action="CPWebcontent#assertElementPresent" locator1="ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute action="CPWebcontent#assertTextEquals" locator1="ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />
	</command>

	<command name="addDraftCP">
		<execute action="CPWebcontent#assertClick" locator1="TOOLBAR_ADD" value1="Add" />
		<execute action="CPWebcontent#assertClick" locator1="ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<execute action="CPWebcontentAddwebcontent#type" locator1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="CPWebcontentAddwebcontent#type" locator1="CONTENT_FIELD" value1="${webContentContent}" />
		<execute action="CPWebcontentAddwebcontent#assertClick" locator1="SAVE_AS_DRAFT_BUTTON" value1="Save as Draft" />
		<execute action="CPWebcontentAddwebcontent#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addNullCP">
		<execute action="CPWebcontent#assertClick" locator1="TOOLBAR_ADD" value1="Add" />
		<execute action="CPWebcontent#assertClick" locator1="ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<if>
			<isset var="webContentContent" />
			<then>
				<execute action="CPWebcontentAddwebcontent#type" locator1="CONTENT_FIELD" value1="${webContentContent}" />
			</then>
		</if>

		<execute action="CPWebcontentAddwebcontent#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />

		<if>
			<condition function="#isElementPresent" locator1="//div[@class='journal-article-body-content']/div[@class='alert alert-error']"  />
			<then>
				<execute action="CPWebcontentAddwebcontent#assertTextEquals" locator1="ERROR_MESSAGE" value1="Your request failed to complete." />
				<execute action="CPWebcontentAddwebcontent#assertTextEquals" locator1="ERROR_MESSAGE_INVALID_NAME" value1="Please enter a valid name." />
			</then>
			<else>
				<execute action="CPWebcontentAddwebcontent#assertTextEquals" locator1="ERROR_MESSAGE_INVALID_NAME" value1="This field is required." />
			</else>
		</if>
	</command>

	<command name="addPGViaAP">
		<execute action="PGAssetpublisher#assertClick" locator1="ADD_NEW_DROPDOWN" value1="Add New" />
		<execute action="PGAssetpublisher#assertClick" locator1="ADD_NEW_MENULIST_WEB_CONTENT" value1="Web Content Article" />

		<execute action="PGAssetpublisherAddnewwebcontent#selectFrame" locator1="WEB_CONTENT_IFRAME" />
		<execute action="PGAssetpublisherAddnewwebcontent#type" locator1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="PGAssetpublisherAddnewwebcontent#type" locator1="CONTENT_FIELD" value1="${webContentContent}" />

		<if>
			<equals arg1="${saveAsDraft}" arg2="true" />
			<then>
				<execute action="PGAssetpublisherAddnewwebcontent#assertClick" locator1="SAVE_AS_DRAFT_BUTTON" value1="Save as Draft" />
				<execute action="PGAssetpublisherAddnewwebcontent#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

				<execute function="#pause" locator1="5000" />
				<execute action="PGAssetpublisher#selectFrame" value1="relative=top" />
				<execute action="PGAssetpublisher#assertElementNotPresent" locator1="ASSET_ABSTRACT_GENERIC" />
			</then>
			<else>
				<execute action="PGAssetpublisherAddnewwebcontent#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />

				<execute function="#pause" locator1="5000" />
				<execute action="PGAssetpublisher#selectFrame" value1="relative=top" />

				<var name="key_assetTitle" value="${webContentTitle}" />

				<execute action="PGAssetpublisher#assertTextEquals" locator1="ASSET_ABSTRACT_TITLE" value1="${webContentTitle}" />
				<execute action="PGAssetpublisher#assertTextEquals" locator1="ASSET_ABSTRACT_SUMMARY" value1="${webContentContent}" />
			</else>
		</if>
	</command>

	<command name="addPGViaPanel">
		<execute action="PGWebcontentdisplayAddwebcontent#selectFrame" locator1="NEW_WEB_CONTENT_ARTICLE_IFRAME" />
		<execute action="PGWebcontentdisplayAddwebcontent#type" locator1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="PGWebcontentdisplayAddwebcontent#type" locator1="CONTENT_FIELD" value1="${webContentContent}" />
		<execute action="PGWebcontentdisplayAddwebcontent#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />
		<execute function="#pause" locator1="5000" />

		<execute action="PGWebcontentdisplay#selectFrame" value1="relative=top" />

		<execute action="PGWebcontentdisplay#assertTextEquals" locator1="WEB_CONTENT_CONTENT" value1="${webContentContent}" />
	</command>

	<command name="addPGViaWCD">
		<execute action="PGWebcontentdisplay#click" locator1="ADD_WEB_CONTENT" value1="Add" />

		<execute action="PGWebcontentdisplayAddwebcontent#type" locator1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="PGWebcontentdisplayAddwebcontent#type" locator1="CONTENT_FIELD" value1="${webContentContent}" />
		<execute action="PGWebcontentdisplayAddwebcontent#click" locator1="PUBLISH_BUTTON" value1="Publish" />

		<if>
			<equals arg1="${webContentJavascript}" arg2="true" />
			<then>
				<execute function="#assertTextEquals" locator1="//div[@class='journal-content-article']//span[contains(@class,'portlet-title-text')]" value1="${embeddedPortletTitle}" />
			</then>
			<else>
				<execute action="PGWebcontentdisplay#assertTextEquals" locator1="WEB_CONTENT_CONTENT" value1="${webContentContent}" />
			</else>
		</if>
	</command>

	<command name="addTagPGViaWCD">
		<execute action="PGWebcontentdisplay#click" locator1="EDIT_WEB_CONTENT" value1="Edit" />

		<execute action="PGWebcontentdisplayEditwebcontent#assertElementPresent" locator1="TOOLBAR_BASIC_PREVIEW" />
		<execute action="PGWebcontentdisplayEditwebcontent#assertClick" locator1="MENU_CATEGORIZATION" value1="Categorization" />

		<var name="key_tagName" value="${tagName}" />

		<execute action="PGWebcontentdisplayEditwebcontentCategorization#type" locator1="CATEGORIZATION_TAGS_FIELD" value1="${tagName}" />
		<execute action="PGWebcontentdisplayEditwebcontentCategorization#assertClick" locator1="CATEGORIZATION_TAGS_ADD_BUTTON" value1="Add" />
		<execute action="PGWebcontentdisplayEditwebcontentCategorization#assertTextEquals" locator1="CATEGORIZATION_TAG" value1="${tagName}" />
		<execute action="PGWebcontentdisplayEditwebcontentCategorization#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />
	</command>

	<command name="addTranslationCP">
		<execute action="CPWebcontent#click" locator1="TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute action="CPWebcontent#assertClick" locator1="DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />

		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute action="CPWebcontent#assertElementPresent" locator1="ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute action="CPWebcontent#assertClick" locator1="ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />

		<var name="key_localizedLanguage" value="${localizedLanguage}" />

		<execute action="CPWebcontentEditwebcontent#assertTextEquals" locator1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="CPWebcontentEditwebcontent#assertClick" locator1="ADD_TRANSLATION" value1="Add Translation" />
		<execute action="CPWebcontentEditwebcontent#assertClick" locator1="ADD_TRANSLATION_MENULIST_LANGUAGE" value1="${localizedLanguage}" />

		<execute action="CPWebcontentEditwebcontentAddtranslation#selectFrame" locator1="WEB_CONTENT_TRANSLATION_IFRAME" />

		<if>
			<isset var="localizedTitle" />
			<then>
				<execute action="CPWebcontentEditwebcontentAddtranslation#type" locator1="TITLE_FIELD" value1="${localizedTitle}" />
				<execute action="CPWebcontentEditwebcontentAddtranslation#type" locator1="CONTENT_FIELD" value1="${localizedContent}" />
			</then>
		</if>

		<if>
			<isset var="localizedInteger" />
			<then>
				<var name="key_fieldName" value="Integer" />

				<execute action="CPWebcontentEditwebcontentAddtranslation#type" locator1="INTEGER_FIELD" value1="${localizedInteger}" />
			</then>
		</if>

		<if>
			<isset var="localizedNumber" />
			<then>
				<var name="key_fieldName" value="Number" />

				<execute action="CPWebcontentEditwebcontentAddtranslation#type" locator1="NUMBER_FIELD" value1="${localizedNumber}" />
			</then>
		</if>

		<execute action="CPWebcontentEditwebcontentAddtranslation#assertClick" locator1="SAVE_BUTTON" value1="Save" />

		<execute action="CPWebcontentEditwebcontent#selectFrame" value1="relative=top" />
		<execute action="CPWebcontentEditwebcontent#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />
	</command>

	<command name="addTranslationPGViaWCD">
		<execute action="PGWebcontentdisplay#click" locator1="EDIT_WEB_CONTENT" value1="Edit" />

		<var name="key_webContentTranslationLanguage" value="${webContentTranslationLanguage}" />

		<execute action="PGWebcontentdisplayEditwebcontent#assertClick" locator1="ADD_TRANSLATION_MENULIST" value1="Add Translation" />
		<execute action="PGWebcontentdisplayEditwebcontent#assertClick" locator1="ADD_TRANSLATION_MENULIST_LANGUAGE" value1="${webContentTranslationLanguage}" />
		<execute action="PGWebcontentdisplayEditwebcontent#selectFrame" locator1="WEB_CONTENT_TRANSLATION_IFRAME" />
		<execute action="PGWebcontentdisplayEditwebcontent#type" locator1="WEB_CONTENT_TRANSLATION_IFRAME_TITLE_FIELD" value1="${webContentTranslationTitle}" />
		<execute action="PGWebcontentdisplayEditwebcontent#type" locator1="WEB_CONTENT_TRANSLATION_IFRAME_CONTENT_FIELD" value1="${webContentTranslationContent}" />
		<execute action="PGWebcontentdisplayEditwebcontent#assertClick" locator1="WEB_CONTENT_TRANSLATION_IFRAME_SAVE_BUTTON" value1="Save" />
		<execute action="PGWebcontentdisplayEditwebcontent#selectFrame" value1="relative=top" />
		<execute action="PGWebcontentdisplayEditwebcontent#assertTextEquals" locator1="AVAILABLE_TRANSLATION" value1="${webContentTranslationLanguage}" />
		<execute action="PGWebcontentdisplayAddwebcontent#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />
	</command>

	<command name="addWithCategoryCP">
		<execute action="CPWebcontent#assertClick" locator1="TOOLBAR_ADD" value1="Add" />
		<execute action="CPWebcontent#assertClick" locator1="ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<execute action="CPWebcontentAddwebcontent#type" locator1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="CPWebcontentAddwebcontent#type" locator1="CONTENT_FIELD" value1="${webContentContent}" />
		<execute action="CPWebcontentAddwebcontent#assertClick" locator1="MENU_CATEGORIZATION" value1="Categorization" />

		<if>
			<isset var="userSegmentDetailTitle" />
			<then>
				<var name="key_userSegmentDetailTitle" value="${userSegmentDetailTitle}" />

				<execute action="CPWebcontentAddwebcontent#assertTextEquals" locator1="USER_SEGMENT_DETAIL_TITLE" value1="${userSegmentDetailTitle}" />
			</then>
		</if>

		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute action="CPWebcontentCategorization#assertClick" locator1="CATEGORY_SELECT" value1="Select" />
		<execute action="CPWebcontentCategorization#click" locator1="CATEGORIES_WINDOW_CATEGORY_CHECKBOX" />
		<execute action="CPWebcontentCategorization#click" locator1="CATEGORIES_WINDOW_CLOSE_BUTTON" />
		<execute action="CPWebcontentCategorization#assertTextEquals" locator1="CATEGORY" value1="${categoryName}" />

		<execute action="CPWebcontentAddwebcontent#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />

		<execute action="CPWebcontent#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute action="CPWebcontent#click" locator1="TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute action="CPWebcontent#assertClick" locator1="DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />
		<execute action="CPWebcontent#assertElementPresent" locator1="ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute action="CPWebcontent#assertTextEquals" locator1="ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />
		<execute action="CPWebcontent#assertTextEquals" locator1="ENTRY_DESCRIPTIVE_CATEGORY" value1="${categoryName}" />
	</command>

	<command name="addWithStructureCP">
		<var name="key_structureName" value="${structureName}" />

		<execute action="CPWebcontent#assertClick" locator1="TOOLBAR_ADD" value1="Add"  />
		<execute action="CPWebcontent#assertClick" locator1="ADD_MENULIST_STRUCTURE_NAME" value1="${structureName}" />

		<execute action="CPWebcontentAddwebcontent#type" locator1="TITLE_FIELD" value1="${webContentTitle}" />

		<if>
			<isset var="webContentBoolean" />
			<then>
				<execute action="CPWebcontentAddwebcontent#check" locator1="BOOLEAN_FIELD" />
			</then>
		</if>

		<if>
			<isset var="webContentDate" />
			<then>
				<execute action="CPWebcontentAddwebcontent#type" locator1="DATE_FIELD" value1="${webContentDate}" />
			</then>
		</if>

		<if>
			<isset var="webContentDecimal" />
			<then>
				<var name="key_fieldName" value="Decimal" />

				<execute action="CPWebcontentAddwebcontent#type" locator1="DECIMAL_FIELD" value1="${webContentDecimal}" />
			</then>
		</if>

		<if>
			<isset var="webContentDocumentsAndMedia" />
			<then>
				<execute action="CPWebcontentAddwebcontent#assertClick" locator1="DOCUMENTS_AND_MEDIA_FIELD_BUTTON" value1="${webContentDocumentsAndMedia}" />
			</then>
		</if>

		<if>
			<isset var="webContentHTML" />
			<then>
				<var name="key_fieldName" value="HTML" />

				<execute action="CPWebcontentAddwebcontent#type" locator1="HTML_FIELD" value1="${webContentHTML}" />
			</then>
		</if>

		<if>
			<isset var="webContentImage" />
			<then>
				<var name="key_fieldName" value="Image" />

				<execute action="CPWebcontentAddwebcontent#uploadCommonFile" locator1="IMAGE_FIELD" value1="${webContentImage}" />
			</then>
		</if>

		<if>
			<isset var="webContentInteger" />
			<then>
				<var name="key_fieldName" value="Integer" />

				<execute action="CPWebcontentAddwebcontent#type" locator1="INTEGER_FIELD" value1="${webContentInteger}" />
			</then>
		</if>

		<if>
			<isset var="webContentLinkToPage" />
			<then>
				<execute action="CPWebcontentAddwebcontent#type" locator1="LINK_TO_PAGE_FIELD" value1="${webContentLinkToPage}" />
			</then>
		</if>

		<if>
			<isset var="webContentNumber" />
			<then>
				<var name="key_fieldName" value="Number" />

				<execute action="CPWebcontentAddwebcontent#type" locator1="NUMBER_FIELD" value1="${webContentNumber}" />
			</then>
		</if>

		<if>
			<isset var="webContentRadio" />
			<then>
				<execute action="CPWebcontentAddwebcontent#select" locator1="RADIO_FIELD" value1="${webContentRadio}" />
			</then>
		</if>

		<if>
			<isset var="webContentSelect" />
			<then>
				<execute action="CPWebcontentAddwebcontent#select" locator1="SELECT_FIELD" value1="${webContentSelect}" />
			</then>
		</if>

		<if>
			<isset var="webContentText" />
			<then>
				<var name="key_fieldName" value="Text" />

				<execute action="CPWebcontentAddwebcontent#type" locator1="TEXT_FIELD" value1="${webContentText}" />
			</then>
		</if>

		<if>
			<isset var="webContentTextBox" />
			<then>
				<execute action="CPWebcontentAddwebcontent#type" locator1="TEXT_BOX_FIELD" value1="${webContentTextBox}" />
			</then>
		</if>

		<execute action="CPWebcontentAddwebcontent#assertClick" locator1="PUBLISH_BUTTON" value1="Publish" />

		<execute action="CPWebcontent#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute action="CPWebcontent#click" locator1="TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute action="CPWebcontent#assertClick" locator1="DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />
		<execute action="CPWebcontent#assertElementPresent" locator1="ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute action="CPWebcontent#assertTextEquals" locator1="ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />
	</command>

	<command name="addWithWorkflowCP">
		<execute action="CPWebcontent#assertClick" locator1="TOOLBAR_ADD" value1="Add"  />
		<execute action="CPWebcontent#assertClick" locator1="ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<execute action="CPWebcontentAddwebcontent#type" locator1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="CPWebcontentAddwebcontent#type" locator1="CONTENT_FIELD" value1="${webContentContent}" />
		<execute action="CPWebcontentAddwebcontent#assertClick" locator1="SUBMIT_FOR_PUBLICATION_BUTTON" value1="Submit for Publication" />

		<execute action="CPWebcontent#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addWithWorkflowPGViaAP">
		<execute action="PGAssetpublisher#assertClick" locator1="ADD_NEW_DROPDOWN" value1="Add New" />
		<execute action="PGAssetpublisher#assertClick" locator1="ADD_NEW_MENULIST_WEB_CONTENT" value1="Web Content Article" />

		<execute action="PGAssetpublisherAddnewwebcontent#selectFrame" locator1="WEB_CONTENT_IFRAME" />
		<execute action="PGAssetpublisherAddnewwebcontent#type" locator1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="PGAssetpublisherAddnewwebcontent#type" locator1="CONTENT_FIELD" value1="${webContentContent}" />
		<execute action="PGAssetpublisherAddnewwebcontent#assertClick" locator1="SUBMIT_FOR_PUBLICATION_BUTTON" value1="Submit for Publication" />
		<execute function="#pause" locator1="5000" />

		<execute action="PGAssetpublisher#selectFrame" />
		<execute action="PGAssetpublisher#assertTextEquals" locator1="INFO_MESSAGE" value1="There are no results." />
	</command>

	<command name="addWithWorkflowPGViaWCD">
		<execute action="PGWebcontentdisplay#click" locator1="ADD_WEB_CONTENT" />

		<execute action="PGWebcontentdisplayAddwebcontent#type" locator1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="PGWebcontentdisplayAddwebcontent#type" locator1="CONTENT_FIELD" value1="${webContentContent}" />
		<execute action="PGWebcontentdisplayAddwebcontent#assertClick" locator1="SUBMIT_FOR_PUBLICATION_BUTTON" value1="Submit for Publication" />

		<execute action="PGWebcontentdisplay#assertTextEquals" locator1="ALERT_MESSAGE" value1="${webContentTitle} is not approved." />
	</command>

	<command name="copyCP">
		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute action="CPWebcontent#check" locator1="ENTRY_DESCRIPTIVE_CHECKBOX" />
		<execute action="CPWebcontent#click" locator1="ENTRY_DESCRIPTIVE_MENULIST_ARROW" />
		<execute action="CPWebcontent#assertClick" locator1="ENTRY_MENULIST_COPY" value1="Copy" />

		<execute action="CPWebcontentCopy#assertClick" locator1="COPY_BUTTON" value1="Copy" />
		<execute action="CPWebcontent#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="CPWebcontent#click" locator1="TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute action="CPWebcontent#assertClick" locator1="DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />
		<execute action="CPWebcontent#assertElementPresent" locator1="ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute action="CPWebcontent#assertTextEquals" locator1="ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />

		<var name="key_categoryName" value="${categoryName}" />

		<execute action="CPWebcontent#assertTextEquals" locator1="ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />
		<execute action="CPWebcontent#assertTextEquals" locator1="ENTRY_DESCRIPTIVE_CATEGORY" value1="${categoryName}" />
		<execute action="CPWebcontent#assertTextEquals" locator1="ENTRY_DESCRIPTIVE_TITLE_COPY" value1="${webContentTitle}" />
		<execute action="CPWebcontent#assertTextEquals" locator1="ENTRY_DESCRIPTIVE_CATEGORY_COPY" value1="${categoryName}" />
	</command>

	<command name="deleteCP">
		<execute macro="WebContent#moveToRecycleBinCP" />

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Recycle Bin" />
		</execute>

		<execute macro="RecycleBin#emptyCP" />

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Web Content" />
		</execute>

		<execute action="CPWebcontent#assertTextNotPresent" value1="${webContentTitle}" />
	</command>

	<command name="editCP">
		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute action="CPWebcontent#click" locator1="TOOLBAR_DISPLAY_STYLE_BUTTON" />
