<definition>
	<command name="addCategoryPGViaWCD">
		<execute function="#click" locator1="PGWebcontentdisplay#EDIT_WEB_CONTENT" value1="Edit" />

		<execute function="#assertClick" locator1="PGWebcontentdisplayEditwebcontent#MENU_CATEGORIZATION" value1="Categorization" />

		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIZATION_SELECT_BUTTON" value1="Select" />
		<execute function="#assertClick" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIES_CATEGORY_UNSELECTED" value1="${categoryName}" />
		<execute function="#assertElementPresent" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIES_CATEGORY_SELECTED" value1="${categoryName}" />
		<execute function="#click" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIES_CLOSE_BUTTON" />
		<execute function="#assertTextEquals" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIZATION_CATEGORY" value1="${categoryName}" />
		<execute function="#assertClick" locator1="PGWebcontentdisplayEditwebcontentCategorization#PUBLISH_BUTTON" value1="Publish" />
	</command>

	<command name="addCP">
		<execute function="#assertClick" locator1="CPWebcontent#TOOLBAR_ADD" value1="Add" />
		<execute function="#assertClick" locator1="CPWebcontent#ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<if>
			<isset var="expirationDate" />
			<then>
				<execute function="#assertClick" locator1="CPWebcontentAddwebcontent#MENU_SCHEDULE" value1="Schedule" />

				<execute function="#assertChecked" locator1="CPWebcontentEditwebcontentSchedule#SCHEDULE_EXPIRATION_DATE_NEVER_EXPIRE_CHECKBOX" value1="Never Expire" />
				<execute function="#uncheck" locator1="CPWebcontentEditwebcontentSchedule#SCHEDULE_EXPIRATION_DATE_NEVER_EXPIRE_CHECKBOX" value1="Never Expire" />
				<execute function="#type" locator1="CPWebcontentEditwebcontentSchedule#SCHEDULE_EXPIRATION_DATE_FIELD" value1="${expirationDate}" />
				<execute function="#assertClick" locator1="CPWebcontentEditwebcontentSchedule#MENU_CONTENT" value1="Content" />
			</then>
		</if>

		<execute function="#type" locator1="CPWebcontentAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="#type" locator1="CPWebcontentAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />
		<execute function="#assertClick" locator1="CPWebcontentAddwebcontent#PUBLISH_BUTTON" value1="Publish" />

		<execute function="#assertTextEquals" locator1="CPWebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute function="#click" locator1="CPWebcontent#TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute function="#assertClick" locator1="CPWebcontent#DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />
		<execute function="#assertElementPresent" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute function="#assertTextEquals" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />
	</command>

	<command name="addDraftCP">
		<execute function="#assertClick" locator1="CPWebcontent#TOOLBAR_ADD" value1="Add" />
		<execute function="#assertClick" locator1="CPWebcontent#ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<execute function="#type" locator1="CPWebcontentAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="#type" locator1="CPWebcontentAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />
		<execute function="#assertClick" locator1="CPWebcontentAddwebcontent#SAVE_AS_DRAFT_BUTTON" value1="Save as Draft" />
		<execute function="#assertTextEquals" locator1="CPWebcontentAddwebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addNullCP">
		<execute function="#assertClick" locator1="CPWebcontent#TOOLBAR_ADD" value1="Add" />
		<execute function="#assertClick" locator1="CPWebcontent#ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<if>
			<isset var="webContentContent" />
			<then>
				<execute function="#type" locator1="CPWebcontentAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />
			</then>
		</if>

		<execute function="#assertClick" locator1="CPWebcontentAddwebcontent#PUBLISH_BUTTON" value1="Publish" />

		<if>
			<condition function="#isElementPresent" locator1="//div[@class='journal-article-body-content']/div[@class='alert alert-error']"  />
			<then>
				<execute function="#assertTextEquals" locator1="CPWebcontentAddwebcontent#ERROR_MESSAGE" value1="Your request failed to complete." />
				<execute function="#assertTextEquals" locator1="CPWebcontentAddwebcontent#ERROR_MESSAGE_INVALID_NAME" value1="Please enter a valid name." />
			</then>
			<else>
				<execute function="#assertTextEquals" locator1="CPWebcontentAddwebcontent#ERROR_MESSAGE_INVALID_NAME" value1="This field is required." />
			</else>
		</if>
	</command>

	<command name="addPGViaAP">
		<execute function="#assertClick" locator1="PGAssetpublisher#ADD_NEW_DROPDOWN" value1="Add New" />
		<execute function="#assertClick" locator1="PGAssetpublisher#ADD_NEW_MENULIST_WEB_CONTENT" value1="Web Content Article" />

		<execute function="#selectFrame" locator1="PGAssetpublisherAddnewwebcontent#WEB_CONTENT_IFRAME" />
		<execute function="#type" locator1="PGAssetpublisherAddnewwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="#type" locator1="PGAssetpublisherAddnewwebcontent#CONTENT_FIELD" value1="${webContentContent}" />

		<if>
			<equals arg1="${saveAsDraft}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="PGAssetpublisherAddnewwebcontent#SAVE_AS_DRAFT_BUTTON" value1="Save as Draft" />
				<execute function="#assertTextEquals" locator1="PGAssetpublisherAddnewwebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />

				<execute function="#pause" locator1="5000" />
				<execute function="#selectFrame" value1="relative=top" />
				<execute function="#assertElementNotPresent" locator1="PGAssetpublisher#ASSET_ABSTRACT_GENERIC" />
			</then>
			<else>
				<execute function="#assertClick" locator1="PGAssetpublisherAddnewwebcontent#PUBLISH_BUTTON" value1="Publish" />

				<execute function="#pause" locator1="5000" />
				<execute function="#selectFrame" value1="relative=top" />

				<var name="key_assetTitle" value="${webContentTitle}" />

				<execute function="#assertTextEquals" locator1="PGAssetpublisher#ASSET_ABSTRACT_TITLE" value1="${webContentTitle}" />
				<execute function="#assertTextEquals" locator1="PGAssetpublisher#ASSET_ABSTRACT_SUMMARY" value1="${webContentContent}" />
			</else>
		</if>
	</command>

	<command name="addPGViaPanel">
		<execute function="#selectFrame" locator1="PGWebcontentdisplayAddwebcontent#NEW_WEB_CONTENT_ARTICLE_IFRAME" />
		<execute function="#type" locator1="PGWebcontentdisplayAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="#type" locator1="PGWebcontentdisplayAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />
		<execute function="#assertClick" locator1="PGWebcontentdisplayAddwebcontent#PUBLISH_BUTTON" value1="Publish" />
		<execute function="#pause" locator1="5000" />

		<execute function="#selectFrame" value1="relative=top" />

		<execute function="#assertTextEquals" locator1="PGWebcontentdisplay#WEB_CONTENT_CONTENT" value1="${webContentContent}" />
	</command>

	<command name="addPGViaWCD">
		<execute function="#click" locator1="PGWebcontentdisplay#ADD_WEB_CONTENT" value1="Add" />

		<execute function="#type" locator1="PGWebcontentdisplayAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="#type" locator1="PGWebcontentdisplayAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />
		<execute function="#click" locator1="PGWebcontentdisplayAddwebcontent#PUBLISH_BUTTON" value1="Publish" />

		<if>
			<equals arg1="${webContentJavascript}" arg2="true" />
			<then>
				<execute function="#assertTextEquals" locator1="//div[@class='journal-content-article']//span[contains(@class,'portlet-title-text')]" value1="${embeddedPortletTitle}" />
			</then>
			<else>
				<execute function="#assertTextEquals" locator1="PGWebcontentdisplay#WEB_CONTENT_CONTENT" value1="${webContentContent}" />
			</else>
		</if>
	</command>

	<command name="addTagPGViaWCD">
		<execute function="#click" locator1="PGWebcontentdisplay#EDIT_WEB_CONTENT" value1="Edit" />

		<execute function="#assertElementPresent" locator1="PGWebcontentdisplayEditwebcontent#TOOLBAR_BASIC_PREVIEW" />
		<execute function="#assertClick" locator1="PGWebcontentdisplayEditwebcontent#MENU_CATEGORIZATION" value1="Categorization" />

		<var name="key_tagName" value="${tagName}" />

		<execute function="#type" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIZATION_TAGS_FIELD" value1="${tagName}" />
		<execute function="#assertClick" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIZATION_TAGS_ADD_BUTTON" value1="Add" />
		<execute function="#assertTextEquals" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIZATION_TAG" value1="${tagName}" />
		<execute function="#assertClick" locator1="PGWebcontentdisplayEditwebcontentCategorization#PUBLISH_BUTTON" value1="Publish" />
	</command>

	<command name="addTranslationCP">
		<execute function="#click" locator1="CPWebcontent#TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute function="#assertClick" locator1="CPWebcontent#DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />

		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute function="#assertElementPresent" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute function="#assertClick" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />

		<var name="key_localizedLanguage" value="${localizedLanguage}" />

		<execute function="#assertTextEquals" locator1="CPWebcontentEditwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="#assertClick" locator1="CPWebcontentEditwebcontent#ADD_TRANSLATION" value1="Add Translation" />
		<execute function="#assertClick" locator1="CPWebcontentEditwebcontent#ADD_TRANSLATION_MENULIST_LANGUAGE" value1="${localizedLanguage}" />

		<execute function="#selectFrame" locator1="CPWebcontentEditwebcontentAddtranslation#WEB_CONTENT_TRANSLATION_IFRAME" />

		<if>
			<isset var="localizedTitle" />
			<then>
				<execute function="#type" locator1="CPWebcontentEditwebcontentAddtranslation#TITLE_FIELD" value1="${localizedTitle}" />
				<execute function="#type" locator1="CPWebcontentEditwebcontentAddtranslation#CONTENT_FIELD" value1="${localizedContent}" />
			</then>
		</if>

		<if>
			<isset var="localizedInteger" />
			<then>
				<var name="key_fieldName" value="Integer" />

				<execute function="#type" locator1="CPWebcontentEditwebcontentAddtranslation#INTEGER_FIELD" value1="${localizedInteger}" />
			</then>
		</if>

		<if>
			<isset var="localizedNumber" />
			<then>
				<var name="key_fieldName" value="Number" />

				<execute function="#type" locator1="CPWebcontentEditwebcontentAddtranslation#NUMBER_FIELD" value1="${localizedNumber}" />
			</then>
		</if>

		<execute function="#assertClick" locator1="CPWebcontentEditwebcontentAddtranslation#SAVE_BUTTON" value1="Save" />

		<execute function="#selectFrame" value1="relative=top" />
		<execute function="#assertClick" locator1="CPWebcontentEditwebcontent#PUBLISH_BUTTON" value1="Publish" />
	</command>

	<command name="addTranslationPGViaWCD">
		<execute function="#click" locator1="PGWebcontentdisplay#EDIT_WEB_CONTENT" value1="Edit" />

		<var name="key_webContentTranslationLanguage" value="${webContentTranslationLanguage}" />

		<execute function="#assertClick" locator1="PGWebcontentdisplayEditwebcontent#ADD_TRANSLATION_MENULIST" value1="Add Translation" />
		<execute function="#assertClick" locator1="PGWebcontentdisplayEditwebcontent#ADD_TRANSLATION_MENULIST_LANGUAGE" value1="${webContentTranslationLanguage}" />
		<execute function="#selectFrame" locator1="PGWebcontentdisplayEditwebcontent#WEB_CONTENT_TRANSLATION_IFRAME" />
		<execute function="#type" locator1="PGWebcontentdisplayEditwebcontent#WEB_CONTENT_TRANSLATION_IFRAME_TITLE_FIELD" value1="${webContentTranslationTitle}" />
		<execute function="#type" locator1="PGWebcontentdisplayEditwebcontent#WEB_CONTENT_TRANSLATION_IFRAME_CONTENT_FIELD" value1="${webContentTranslationContent}" />
		<execute function="#assertClick" locator1="PGWebcontentdisplayEditwebcontent#WEB_CONTENT_TRANSLATION_IFRAME_SAVE_BUTTON" value1="Save" />
		<execute function="#selectFrame" value1="relative=top" />
		<execute function="#assertTextEquals" locator1="PGWebcontentdisplayEditwebcontent#AVAILABLE_TRANSLATION" value1="${webContentTranslationLanguage}" />
		<execute function="#assertClick" locator1="PGWebcontentdisplayAddwebcontent#PUBLISH_BUTTON" value1="Publish" />
	</command>

	<command name="addWithCategoryCP">
		<execute function="#assertClick" locator1="CPWebcontent#TOOLBAR_ADD" value1="Add" />
		<execute function="#assertClick" locator1="CPWebcontent#ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<execute function="#type" locator1="CPWebcontentAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="#type" locator1="CPWebcontentAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />
		<execute function="#assertClick" locator1="CPWebcontentAddwebcontent#MENU_CATEGORIZATION" value1="Categorization" />

		<if>
			<isset var="userSegmentDetailTitle" />
			<then>
				<var name="key_userSegmentDetailTitle" value="${userSegmentDetailTitle}" />

				<execute function="#assertTextEquals" locator1="CPWebcontentAddwebcontent#USER_SEGMENT_DETAIL_TITLE" value1="${userSegmentDetailTitle}" />
			</then>
		</if>

		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="#assertClick" locator1="CPWebcontentCategorization#CATEGORY_SELECT" value1="Select" />
		<execute function="#click" locator1="CPWebcontentCategorization#CATEGORIES_WINDOW_CATEGORY_CHECKBOX" />
		<execute function="#click" locator1="CPWebcontentCategorization#CATEGORIES_WINDOW_CLOSE_BUTTON" />
		<execute function="#assertTextEquals" locator1="CPWebcontentCategorization#CATEGORY" value1="${categoryName}" />

		<execute function="#assertClick" locator1="CPWebcontentAddwebcontent#PUBLISH_BUTTON" value1="Publish" />

		<execute function="#assertTextEquals" locator1="CPWebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute function="#click" locator1="CPWebcontent#TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute function="#assertClick" locator1="CPWebcontent#DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />
		<execute function="#assertElementPresent" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute function="#assertTextEquals" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />
		<execute function="#assertTextEquals" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_CATEGORY" value1="${categoryName}" />
	</command>

	<command name="addWithStructureCP">
		<var name="key_structureName" value="${structureName}" />

		<execute function="#assertClick" locator1="CPWebcontent#TOOLBAR_ADD" value1="Add"  />
		<execute function="#assertClick" locator1="CPWebcontent#ADD_MENULIST_STRUCTURE_NAME" value1="${structureName}" />

		<execute function="#type" locator1="CPWebcontentAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />

		<if>
			<isset var="webContentBoolean" />
			<then>
				<execute function="#check" locator1="CPWebcontentAddwebcontent#BOOLEAN_FIELD" />
			</then>
		</if>

		<if>
			<isset var="webContentDate" />
			<then>
				<execute function="#type" locator1="CPWebcontentAddwebcontent#DATE_FIELD" value1="${webContentDate}" />
			</then>
		</if>

		<if>
			<isset var="webContentDecimal" />
			<then>
				<var name="key_fieldName" value="Decimal" />

				<execute function="#type" locator1="CPWebcontentAddwebcontent#DECIMAL_FIELD" value1="${webContentDecimal}" />
			</then>
		</if>

		<if>
			<isset var="webContentDocumentsAndMedia" />
			<then>
				<execute function="#assertClick" locator1="CPWebcontentAddwebcontent#DOCUMENTS_AND_MEDIA_FIELD_BUTTON" value1="${webContentDocumentsAndMedia}" />
			</then>
		</if>

		<if>
			<isset var="webContentHTML" />
			<then>
				<var name="key_fieldName" value="HTML" />

				<execute function="#type" locator1="CPWebcontentAddwebcontent#HTML_FIELD" value1="${webContentHTML}" />
			</then>
		</if>

		<if>
			<isset var="webContentImage" />
			<then>
				<var name="key_fieldName" value="Image" />

				<execute function="#uploadCommonFile" locator1="CPWebcontentAddwebcontent#IMAGE_FIELD" value1="${webContentImage}" />
			</then>
		</if>

		<if>
			<isset var="webContentInteger" />
			<then>
				<var name="key_fieldName" value="Integer" />

				<execute function="#type" locator1="CPWebcontentAddwebcontent#INTEGER_FIELD" value1="${webContentInteger}" />
			</then>
		</if>

		<if>
			<isset var="webContentLinkToPage" />
			<then>
				<execute function="#type" locator1="CPWebcontentAddwebcontent#LINK_TO_PAGE_FIELD" value1="${webContentLinkToPage}" />
			</then>
		</if>

		<if>
			<isset var="webContentNumber" />
			<then>
				<var name="key_fieldName" value="Number" />

				<execute function="#type" locator1="CPWebcontentAddwebcontent#NUMBER_FIELD" value1="${webContentNumber}" />
			</then>
		</if>

		<if>
			<isset var="webContentRadio" />
			<then>
				<execute function="#select" locator1="CPWebcontentAddwebcontent#RADIO_FIELD" value1="${webContentRadio}" />
			</then>
		</if>

		<if>
			<isset var="webContentSelect" />
			<then>
				<execute function="#select" locator1="CPWebcontentAddwebcontent#SELECT_FIELD" value1="${webContentSelect}" />
			</then>
		</if>

		<if>
			<isset var="webContentText" />
			<then>
				<var name="key_fieldName" value="Text" />

				<execute function="#type" locator1="CPWebcontentAddwebcontent#TEXT_FIELD" value1="${webContentText}" />
			</then>
		</if>

		<if>
			<isset var="webContentTextBox" />
			<then>
				<execute function="#type" locator1="CPWebcontentAddwebcontent#TEXT_BOX_FIELD" value1="${webContentTextBox}" />
			</then>
		</if>

		<execute function="#assertClick" locator1="CPWebcontentAddwebcontent#PUBLISH_BUTTON" value1="Publish" />

		<execute function="#assertTextEquals" locator1="CPWebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute function="#click" locator1="CPWebcontent#TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute function="#assertClick" locator1="CPWebcontent#DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />
		<execute function="#assertElementPresent" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute function="#assertTextEquals" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />
	</command>

	<command name="addWithWorkflowCP">
		<execute function="#assertClick" locator1="CPWebcontent#TOOLBAR_ADD" value1="Add"  />
		<execute function="#assertClick" locator1="CPWebcontent#ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<execute function="#type" locator1="CPWebcontentAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
