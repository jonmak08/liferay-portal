	<command name="copyCP">
		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute function="#check" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_CHECKBOX" />
		<execute function="#click" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_MENULIST_ARROW" />
		<execute function="#assertClick" locator1="CPWebcontent#ENTRY_MENULIST_COPY" value1="Copy" />

		<execute function="#assertClick" locator1="CPWebcontentCopy#COPY_BUTTON" value1="Copy" />
