<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 7.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_7_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>7.0.0+</version>
	</compatibility>
	<theme id="alex-rb" name="AlexRB">
		<template-extension>ftl</template-extension>
		<portlet-decorator id="barebone" name="Barebone">
			<portlet-decorator-css-class>portlet-barebone</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="borderless" name="Borderless">
			<portlet-decorator-css-class>portlet-borderless</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="dark-fade" name="Dark Fade">
			<portlet-decorator-css-class>portlet-barebone portlet-dark portlet-fade portlet-rb</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="decorate" name="Decorate">
			<default-portlet-decorator>true</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-decorate</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="fade" name="Fade">
			<portlet-decorator-css-class>portlet-fade portlet-rb</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="light-fade" name="Light Fade">
			<portlet-decorator-css-class>portlet-barebone portlet-fade portlet-light portlet-rb</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="push" name="Push">
			<portlet-decorator-css-class>portlet-push portlet-rb</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="static" name="Static">
			<portlet-decorator-css-class>portlet-rb</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="zoom" name="Zoom">
			<portlet-decorator-css-class>portlet-rb portlet-zoom</portlet-decorator-css-class>
		</portlet-decorator>
	</theme>
</look-and-feel>
